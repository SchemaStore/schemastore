{
  "$id": "https://json.schemastore.org/tldr.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "platform": {
      "oneOf": [
        {
          "enum": ["linux"]
        },
        {
          "type": "string"
        }
      ]
    },
    "pagesRepository": {
      "type": "string"
    },
    "repository": {
      "type": "string"
    },
    "skipUpdateWhenPageNotFound": {
      "type": "boolean"
    },
    "theme": {
      "type": "string"
    },
    "themes": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "commandName": {
            "type": "string"
          },
          "mainDescription": {
            "type": "string"
          },
          "exampleDescription": {
            "type": "string"
          },
          "exampleCode": {
            "type": "string"
          },
          "exampleToken": {
            "type": "string"
          }
        }
      }
    }
  },
  "type": "object"
}
