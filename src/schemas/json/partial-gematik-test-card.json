{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://json.schemastore.org/partial-gematik-test-card.json",
  "title": "Card",
  "description": "Card details",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "iccsn": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "expiryDate": {
      "type": "string",
      "pattern": "^\\d{4}-(0?[1-9]|1[012])-(0?[1-9]|[12][0-9]|3[01])$"
    }
  },
  "required": ["iccsn", "type"]
}
