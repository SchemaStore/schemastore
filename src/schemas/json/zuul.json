          "title": "Default-Branch",
          "type": "string"
        },
        "description": {
          "title": "Description",
          "type": "string"
        },
        "gate": {
          "$ref": "#/definitions/PipelineModel"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "periodic-weekly": {
          "$ref": "#/definitions/PipelineModel"
        },
        "post": {
          "$ref": "#/definitions/PipelineModel"
        },
        "promote": {
          "$ref": "#/definitions/PipelineModel"
        },
        "release": {
          "$ref": "#/definitions/PipelineModel"
        },
        "templates": {
          "items": {
            "type": "string"
          },
          "title": "Templates",
          "type": "array"
        },
        "third-party-check": {
          "$ref": "#/definitions/PipelineModel"
        },
        "vars": {
          "title": "Vars",
          "type": "object"
        }
