  "examples": ["zuul.d/*.yaml", "zuul-tests.d/*.yaml", ".zuul.yaml"],
  "items": {
    "anyOf": [
      {
        "$ref": "#/definitions/JobEntry"
      },
      {
        "$ref": "#/definitions/ProjectEntry"
