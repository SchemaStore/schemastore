          "title": "Tags"
        },
        "timeout": {
          "title": "Timeout",
          "type": "integer"
        },
        "vars": {
          "title": "Vars",
          "type": "object"
        },
        "voting": {
          "default": true,
          "title": "Voting",
          "type": "boolean"
        }
      },
      "required": ["name"],
      "title": "JobModel",
      "type": "object"
    },
    "JobDependencyModel": {
      "additionalProperties": false,
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
