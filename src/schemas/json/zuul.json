        "description": {
          "title": "Description",
          "type": "string"
        },
        "gate": {
          "$ref": "#/definitions/PipelineModel"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "periodic-weekly": {
          "$ref": "#/definitions/PipelineModel"
        },
        "post": {
          "$ref": "#/definitions/PipelineModel"
        },
        "promote": {
          "$ref": "#/definitions/PipelineModel"
        },
        "release": {
          "$ref": "#/definitions/PipelineModel"
        },
        "third-party-check": {
