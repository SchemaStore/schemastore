          "title": "Required-Projects",
          "type": "array"
        },
        "requires": {
          "items": {
            "type": "string"
          },
          "title": "Requires",
          "type": "array"
        },
        "roles": {
          "items": {
            "$ref": "#/definitions/ZuulRoleModel"
          },
          "title": "Roles",
          "type": "array"
        },
        "run": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Run"
        },
        "secrets": {
          "anyOf": [
            {
              "$ref": "#/definitions/JobSecretModel"
            },
            {
              "items": {
                "anyOf": [
                  {
                    "$ref": "#/definitions/JobSecretModel"
                  },
                  {
                    "type": "string"
                  }
                ]
              },
              "type": "array"
            }
          ],
          "title": "Secrets"
        },
        "success-url": {
          "title": "Success-Url",
          "type": "string"
