          "default": false,
          "title": "Soft",
          "type": "boolean"
        }
      },
      "required": ["name"],
      "title": "JobDependencyModel",
      "type": "object"
    },
    "JobSecretModel": {
      "additionalProperties": false,
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "pass-to-parent": {
          "default": false,
          "title": "Pass-To-Parent",
          "type": "boolean"
