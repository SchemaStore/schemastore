          "title": "Post-Run"
        },
        "post-timeout": {
          "title": "Post-Timeout",
          "type": "integer"
        },
        "pre-run": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Pre-Run"
        },
        "provides": {
          "items": {
            "type": "string"
          },
          "title": "Provides",
          "type": "array"
        },
        "required-projects": {
          "items": {
            "anyOf": [
              {
                "type": "string"
