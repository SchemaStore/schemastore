        "irrelevant-files": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Irrelevant-Files"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "nodeset": {
          "title": "Nodeset"
        },
        "override-checkout": {
          "title": "Override-Checkout",
          "type": "string"
        },
        "parent": {
          "title": "Parent",
          "type": "string"
        },
        "post-run": {
          "anyOf": [
            {
              "type": "string"
