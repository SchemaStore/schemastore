{
  "title": "JSON schema for NLU (npm-link-up) library.",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "list","packages","deps"
  ],
  "definitions": {
    "PackagesMap": {
      "additionalProperties": {
      },
      "type": "object"
    }
  },
  "properties": {
    "$schema": {
      "type": "string"
    },
    "alwaysReinstall": {
      "type": "boolean"
    },
    "comments": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "deps": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "ignore": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "linkToItself": {
      "type": "boolean"
    },
    "linkable": {
      "enum": [
        true
      ],
      "type": "boolean"
    },
    "list": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "localSettings": {
      "additionalProperties": {
      },
      "type": "object"
    },
    "npm-link-up": {
      "type": "boolean"
    },
    "packages": {
      "$ref": "#/definitions/PackagesMap"
    },
    "searchRoots": {
      "items": {
        "type": "string"
      },
      "type": "array"
    }
  }
}

