            "lukewarm_ocean",
            "modified_badlands_plateau",
            "modified_gravelly_mountains",
            "modified_jungle",
            "modified_jungle_edge",
            "modified_wooded_badlands_plateau",
            "mountain_edge",
            "mountains",
            "mushroom_field_shore",
            "mushroom_fields",
            "nether",
