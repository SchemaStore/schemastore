            "river",
            "savanna",
            "savanna_plateau",
            "shattered_savanna",
            "shattered_savanna_plateau",
            "small_end_islands",
            "snowy_beach",
            "snowy_mountains",
            "snowy_taiga",
            "snowy_taiga_hills",
            "snowy_taiga_mountains",
            "snowy_tundra",
            "stone_shore",
            "sunflower_plains",
            "swamp",
