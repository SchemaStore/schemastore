    "oneOrMoreDiagnostics": {
      "oneOf": [
        {
          "anyOf": [
            {
              "type": "string",
              "minLength": 1
            },
            {
              "$ref": "#/definitions/clangTidyCheck"
            }
          ]
        },
        {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "description": "A diagnostic\nhttps://clangd.llvm.org/config.html#clangtidy",
            "anyOf": [
              {
                "type": "string",
                "minLength": 1
              },
              {
                "$ref": "#/definitions/clangTidyCheck"
              }
            ]
          }
        }
      ]
    }
