{
  "title": "JSON schema for Azure Webjobs collection files",
  "$schema": "http://json-schema.org/draft-04/schema#",

  "type": "object",
  "required": [ "WebJobs" ],

  "properties": {
    "WebJobs": {
      "type": "array",
      "description": "A list of Azure Webjobs.",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "filePath": {
            "type": "string"
          }
        }
      }
    }
  }
}
