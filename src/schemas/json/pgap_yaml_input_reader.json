{
    "$id": "http://example.com/example.json",
    "type": "object",
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Schema for reading JSON files converted from YAML files described in PGAPX-134. Originally automatically generated, with manually added 'required' elements and 'enum' elements",
    "required": [
        "authors",
        "contact_info"
    ],
    "properties": {
        "consortium": {
            "$id": "/properties/consortium",
            "type": "string",
            "title": "The Consortium Schema ",
            "default": "",
            "examples": ["SkyNet"]
        },
        "comment": {
            "$id": "/properties/comment",
            "type": "string",
            "title": "The Comment Schema ",
            "default": "",
            "examples": ["This draft WGS assembly was generated by running SKESA to  generate a de-novo assembly. The de-novo assembly was then concatenated with  contigs generated using a guided assembler using antimicrobial resistance genes as baits to comprehensively catalog the set of resistance genes in the isolate. Note, some parts of the contigs derived from the guided assembler may overlap de-novo contigs, and other guided assembler contigs. De-novo contigs can be differentiated from guided assembler contigs by their names, which include either 'denovo' or 'guided'."]
        },
        "tp_assembly": {
            "$id": "/properties/tp_assembly",
            "type": "boolean",
            "title": "The Third Party Assembly Schema ",
            "default": false,
            "examples": [false]
        },
        "sra": {
            "$id": "/properties/sra",
            "type": "array",
            "items": {
                "$id": "/properties/sra/items",
                "type": "object",
                "required": ["accession"],
                "properties": {
                    "accession": {
                        "$id": "/properties/sra/items/properties/accession",
                        "title": "The Accession Schema ",
                        "type": "string",
                        "default": "",
                        "examples": ["SRR8796989"]
                    }
                }
            }
        },
        "authors": {
            "$id": "/properties/authors",
            "type": "array",
            "items": {
                "$id": "/properties/authors/items",
                "type": "object",
                "required": ["author"],
                "properties": {
                    "author": {
                        "$id": "/properties/authors/items/properties/author",
                        "type": "object",
                        "required": [
                            "first_name",
                            "last_name"
                        ],
                        "properties": {
                            "first_name": {
                                "$id": "/properties/authors/items/properties/author/properties/first_name",
                                "type": "string",
                                "title": "The First_name Schema ",
                                "default": "",
                                "examples": ["Arnold"]
                            },
                            "last_name": {
                                "$id": "/properties/authors/items/properties/author/properties/last_name",
                                "type": "string",
                                "title": "The Last_name Schema ",
                                "default": "",
                                "examples": ["Schwarzenegger"]
                            },
                            "middle_initial": {
                                "$id": "/properties/authors/items/properties/author/properties/middle_initial",
                                "type": "string",
                                "title": "The Middle_initial Schema ",
                                "default": "",
                                "examples": ["T800"]
                            }
                        }
                    }
                }
            }
        },
        "bioproject": {
            "$id": "/properties/bioproject",
            "type": "string",
            "title": "The Bioproject Schema ",
            "default": "",
            "examples": ["PRJ9999999"]
        },
        "biosample": {
            "$id": "/properties/biosample",
            "type": "string",
            "title": "The Biosample Schema ",
            "default": "",
            "examples": ["SAMN99999999"]
        },
        "contact_info": {
            "$id": "/properties/contact_info",
            "type": "object",
            "required": [
                "organization",
                "department",
                "city",
                "country",
                "street",
                "email",
                "first_name",
                "last_name",
                "postal_code"
            ],
            "properties": {
                "state": {
                    "$id": "/properties/contact_info/properties/state",
                    "type": "string",
                    "title": "The State Schema ",
                    "default": "",
                    "examples": [
                        "MD",
                        "Florida"
                    ]
                },
                "fax": {
                    "$id": "/properties/contact_info/properties/fax",
                    "type": "string",
                    "title": "The Fax Schema ",
                    "default": "",
                    "examples": [
                        "301-555-1234",
                        "+7 095 555 1234"
                    ]
                },
                "city": {
                    "$id": "/properties/contact_info/properties/city",
                    "type": "string",
                    "title": "The City Schema ",
                    "default": "",
                    "examples": ["Docker"]
                },
                "country": {
                    "$id": "/properties/contact_info/properties/country",
                    "type": "string",
                    "title": "The Country Schema ",
                    "default": "",
                    "examples": ["Lappland"]
                },
                "department": {
                    "$id": "/properties/contact_info/properties/department",
                    "type": "string",
                    "title": "The Department Schema ",
                    "default": "",
                    "examples": ["Department of Using NCBI"]
                },
                "email": {
                    "$id": "/properties/contact_info/properties/email",
                    "type": "string",
                    "title": "The Email Schema ",
                    "default": "",
                    "examples": ["jane_doe@gmail.com"]
                },
                "first_name": {
                    "$id": "/properties/contact_info/properties/first_name",
                    "type": "string",
                    "title": "The First_name Schema ",
                    "default": "",
                    "examples": ["Jane"]
                },
                "middle_initial": {
                    "$id": "/properties/contact_info/properties/middle_initial",
                    "type": "string",
                    "title": "The Middle_initial Schema ",
                    "default": "",
                    "examples": ["N"]
                },
                "last_name": {
                    "$id": "/properties/contact_info/properties/last_name",
                    "type": "string",
                    "title": "The Last_name Schema ",
                    "default": "",
                    "examples": ["Doe"]
                },
                "organization": {
                    "$id": "/properties/contact_info/properties/organization",
                    "type": "string",
                    "title": "The Organization Schema ",
                    "default": "",
                    "examples": ["Institute of Klebsiella foobarensis research"]
                },
                "phone": {
                    "$id": "/properties/contact_info/properties/phone",
                    "type": "string",
                    "title": "The Phone Schema ",
                    "default": "",
                    "examples": ["301-555-0245"]
                },
                "postal_code": {
                    "$id": "/properties/contact_info/properties/postal_code",
                    "type": "string",
                    "title": "The Postal_code Schema ",
                    "default": "",
                    "examples": ["12345"]
                },
                "street": {
                    "$id": "/properties/contact_info/properties/street",
                    "type": "string",
                    "title": "The Street Schema ",
                    "default": "",
                    "examples": ["1234 Main St"]
                }
            }
        },
        "fasta": {
            "$id": "/properties/fasta",
            "type": "object",
            "properties": {
                "class": {
                    "$id": "/properties/fasta/properties/class",
                    "type": "string",
                    "title": "The Class Schema ",
                    "default": "",
                    "examples": ["File"]
                },
                "location": {
                    "$id": "/properties/fasta/properties/location",
                    "type": "string",
                    "title": "The Location Schema ",
                    "default": "",
                    "examples": ["sample_fasta_input.fasta"]
                }
            }
        },
        "locus_tag_prefix": {
            "$id": "/properties/locus_tag_prefix",
            "type": "string",
            "title": "The Locus_tag_prefix Schema ",
            "default": "",
            "examples": ["tmp"]
        },
        "organism": {
            "$id": "/properties/organism",
            "type": "object",
            "properties": {
                "strain": {
                    "$id": "/properties/organism/properties/strain",
                    "type": "string",
                    "title": "The Strain Schema ",
                    "default": "",
                    "examples": ["my_strain"]
                },
                "genus_species": {
                    "$id": "/properties/organism/properties/genus_species",
                    "type": "string",
                    "title": "The Genus Species Schema ",
                    "default": "",
                    "examples": ["Escherichia coli"]
                }
            }
        },
        "publications": {
            "$id": "/properties/publications",
            "type": "array",
            "items": {
                "$id": "/properties/publications/items",
                "type": "object",
                "required": ["publication"],
                "properties": {
                    "publication": {
                        "$id": "/properties/publications/items/properties/publication",
                        "type": "object",
                        "properties": {
                            "authors": {
                                "$id": "/properties/publications/items/properties/publication/properties/authors",
                                "type": "array",
                                "items": {
                                    "$id": "/properties/publications/items/properties/publication/properties/authors/items",
                                    "type": "object",
                                    "properties": {
                                        "author": {
                                            "$id": "/properties/publications/items/properties/publication/properties/authors/items/properties/author",
                                            "type": "object",
                                            "required": [
                                                "first_name",
                                                "last_name"
                                            ],
                                            "properties": {
                                                "first_name": {
                                                    "$id": "/properties/publications/items/properties/publication/properties/authors/items/properties/author/properties/first_name",
                                                    "type": "string",
                                                    "title": "The First_name Schema ",
                                                    "default": "",
                                                    "examples": ["Arnold"]
                                                },
                                                "last_name": {
                                                    "$id": "/properties/publications/items/properties/publication/properties/authors/items/properties/author/properties/last_name",
                                                    "type": "string",
                                                    "title": "The Last_name Schema ",
                                                    "default": "",
                                                    "examples": ["Schwarzenegger"]
                                                },
                                                "middle_initial": {
                                                    "$id": "/properties/publications/items/properties/publication/properties/authors/items/properties/author/properties/middle_initial",
                                                    "type": "string",
                                                    "title": "The Middle_initial Schema ",
                                                    "default": "",
                                                    "examples": ["T800"]
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "status": {
                                "$id": "/properties/publications/items/properties/publication/properties/status",
                                "type": "string",
                                "title": "The Status Schema ",
                                "default": "",
                                "enum": [
                                    "published",
                                    "in-press",
                                    "unpublished"
                                ]
                            },
                            "pmid": {
                                "$id": "/properties/publications/items/properties/publication/properties/pmid",
                                "type": "integer",
                                "title": "The Status Schema ",
                                "default": ""
                            },
                            "title": {
                                "$id": "/properties/publications/items/properties/publication/properties/title",
                                "type": "string",
                                "title": "The Title Schema ",
                                "default": "",
                                "examples": ["Discrete CHARMm of Klebsiella foobarensis. Journal of Improbable Results, vol. 34, issue 13, pages: 10001-100005, 2018"]
                            }
                        }
                    }
                }
            }
        },
        "topology": {
            "$id": "/properties/topology",
            "type": "string",
            "title": "The Topology Schema ",
            "default": "",
            "examples": [
                "circular",
                "linear"
            ]
        }
    }
}
