{
  "alerting": {
    "alertmanagers": [
      {
        "scheme": "https",
        "static_configs": [
          {
            "targets": ["1.2.3.4:9093", "1.2.3.5:9093", "1.2.3.6:9093"]
          }
        ]
      }
    ]
  },
  "global": {
    "evaluation_interval": "30s",
    "external_labels": {
      "foo": "bar",
      "monitor": "codelab"
    },
    "query_log_file": "query.log",
    "scrape_interval": "15s"
  },
  "remote_read": [
    {
      "headers": {
        "User-Agent": "Prometheus"
      },
      "name": "remote-read-0",
      "read_recent": true,
      "url": "http://remote1/read"
    },
    {
      "read_recent": false,
      "required_matchers": {
        "job": "special"
      },
      "tls_config": {
        "cert_file": "valid_cert_file",
        "key_file": "valid_key_file"
      },
      "url": "http://remote3/read"
    }
  ],
  "remote_write": [
    {
      "headers": {
        "User-Agent": "Prometheus"
      },
      "metadata_config": {
        "send_interval": "2m"
      },
      "name": "remote-write-0",
      "send_exemplars": true,
      "url": "http://remote1/push",
      "write_relabel_configs": [
        {
          "action": "drop",
          "regex": "expensive.*",
          "source_labels": ["__name__"]
        }
      ]
    },
    {
      "tls_config": {
        "cert_file": "valid_cert_file",
        "key_file": "valid_key_file"
      },
      "url": "http://remote2/push"
    }
  ],
  "rule_files": ["first.rules", "my/*.rules"],
  "scrape_config_files": ["first.yml", "scrape_configs/*.yml"],
  "scrape_configs": [
    {
      "bearer_token_file": "valid_token_file",
      "file_sd_configs": [
        {
          "files": ["foo/*.slow.json", "foo/*.slow.yml", "single/file.yml"],
          "refresh_interval": "10m"
        },
        {
          "files": ["bar/*.yaml"]
        }
      ],
      "honor_labels": true,
      "job_name": "prometheus",
      "relabel_configs": [
        {
          "regex": "(.*)some-[regex]",
          "replacement": "foo-${1}",
          "source_labels": ["job", "__meta_dns_name"],
          "target_label": "job"
        },
        {
          "source_labels": ["abc"],
          "target_label": "cde"
        },
        {
          "replacement": "static",
          "target_label": "abc"
        },
        {
          "regex": null,
          "replacement": "static",
          "target_label": "abc"
        }
      ],
      "static_configs": [
        {
          "labels": {
            "my": "label",
            "your": "label"
          },
          "targets": ["localhost:9090", "localhost:9191"]
        }
      ]
    },
    {
      "basic_auth": {
        "password": "multiline\nmysecret\ntest",
        "username": "admin_name"
      },
      "dns_sd_configs": [
        {
          "names": [
            "first.dns.address.domain.com",
            "second.dns.address.domain.com"
          ],
          "refresh_interval": "15s"
        },
        {
          "names": ["first.dns.address.domain.com"]
        }
      ],
      "job_name": "service-x",
      "metric_relabel_configs": [
        {
          "action": "drop",
          "regex": "expensive_metric.*",
          "source_labels": ["__name__"]
        }
      ],
      "metrics_path": "/my_path",
      "relabel_configs": [
        {
          "action": "drop",
          "regex": "(.*)some-[regex]",
          "source_labels": ["job"]
        },
        {
          "action": "hashmod",
          "modulus": 8,
          "source_labels": ["__address__"],
          "target_label": "__tmp_hash"
        },
        {
          "action": "keep",
          "regex": 1,
          "source_labels": ["__tmp_hash"]
        },
        {
          "action": "labelmap",
          "regex": 1
        },
        {
          "action": "labeldrop",
          "regex": "d"
        },
        {
          "action": "labelkeep",
          "regex": "k"
        }
      ],
      "sample_limit": 1000,
      "scheme": "https",
      "scrape_interval": "50s",
      "scrape_timeout": "5s"
    },
    {
      "consul_sd_configs": [
        {
          "allow_stale": true,
          "node_meta": {
            "rack": "123"
          },
          "scheme": "https",
          "server": "localhost:1234",
          "services": ["nginx", "cache", "mysql"],
          "tags": ["canary", "v1"],
          "tls_config": {
            "ca_file": "valid_ca_file",
            "cert_file": "valid_cert_file",
            "insecure_skip_verify": false,
            "key_file": "valid_key_file"
          },
          "token": "mysecret"
        }
      ],
      "job_name": "service-y",
      "relabel_configs": [
        {
          "regex": "label:([^=]+)=([^,]+)",
          "replacement": "${2}",
          "separator": ",",
          "source_labels": ["__meta_sd_consul_tags"],
          "target_label": "${1}"
        }
      ]
    },
    {
      "bearer_token": "mysecret",
      "job_name": "service-z",
      "tls_config": {
        "cert_file": "valid_cert_file",
        "key_file": "valid_key_file"
      }
    },
    {
      "job_name": "service-kubernetes",
      "kubernetes_sd_configs": [
        {
          "api_server": "https://localhost:1234",
          "basic_auth": {
            "password": "mysecret",
            "username": "myusername"
          },
          "role": "endpoints",
          "tls_config": {
            "cert_file": "valid_cert_file",
            "key_file": "valid_key_file"
          }
        }
      ]
    },
    {
      "basic_auth": {
        "password_file": "valid_password_file",
        "username": "myusername"
      },
      "job_name": "service-kubernetes-namespaces",
      "kubernetes_sd_configs": [
        {
          "api_server": "https://localhost:1234",
          "namespaces": {
            "names": ["default"]
          },
          "role": "endpoints"
        }
      ]
    },
    {
      "job_name": "service-marathon",
      "marathon_sd_configs": [
        {
          "auth_token": "mysecret",
          "servers": ["https://marathon.example.com:443"],
          "tls_config": {
            "cert_file": "valid_cert_file",
            "key_file": "valid_key_file"
          }
        }
      ]
    },
    {
      "ec2_sd_configs": [
        {
          "access_key": "access",
          "filters": [
            {
              "name": "tag:environment",
              "values": ["prod"]
            },
            {
              "name": "tag:service",
              "values": ["web", "db"]
            }
          ],
          "profile": "profile",
          "region": "us-east-1",
          "secret_key": "mysecret"
        }
      ],
      "job_name": "service-ec2"
    },
    {
      "azure_sd_configs": [
        {
          "authentication_method": "OAuth",
          "client_id": "333333CC-3C33-3333-CCC3-33C3CCCCC33C",
          "client_secret": "mysecret",
          "environment": "AzurePublicCloud",
          "port": 9100,
          "subscription_id": "11AAAA11-A11A-111A-A111-1111A1111A11",
          "tenant_id": "BBBB222B-B2B2-2B22-B222-2BB2222BB2B2"
        }
      ],
      "job_name": "service-azure"
    },
    {
      "job_name": "service-nerve",
      "nerve_sd_configs": [
        {
          "paths": ["/monitoring"],
          "servers": ["localhost"]
        }
      ]
    },
    {
      "job_name": "0123service-xxx",
      "metrics_path": "/metrics",
      "static_configs": [
        {
          "targets": ["localhost:9090"]
        }
      ]
    },
    {
      "honor_timestamps": false,
      "job_name": "badfederation",
      "metrics_path": "/federate",
      "static_configs": [
        {
          "targets": ["localhost:9090"]
        }
      ]
    },
    {
      "job_name": "測試",
      "metrics_path": "/metrics",
      "static_configs": [
        {
          "targets": ["localhost:9090"]
        }
      ]
    },
    {
      "job_name": "service-triton",
      "triton_sd_configs": [
        {
          "account": "testAccount",
          "dns_suffix": "triton.example.com",
          "endpoint": "triton.example.com",
          "port": 9163,
          "refresh_interval": "1m",
          "tls_config": {
            "cert_file": "valid_cert_file",
            "key_file": "valid_key_file"
          },
          "version": 1
        }
      ]
    },
    {
      "job_name": "service-openstack",
      "openstack_sd_configs": [
        {
          "port": 80,
          "refresh_interval": "1m",
          "region": "RegionOne",
          "role": "instance",
          "tls_config": {
            "ca_file": "valid_ca_file",
            "cert_file": "valid_cert_file",
            "key_file": "valid_key_file"
          }
        }
      ]
    }
  ],
  "storage": {
    "exemplars": {
      "max_exemplars": 200000
    },
    "tsdb": {
      "out_of_order_time_window": "10m"
    }
  }
}
