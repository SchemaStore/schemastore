{
  "version": "~> 1.0.0",
  "import": [
    {
      "source": ".travis/other.yml",
      "mode": "deep_merge",
      "if": "branch != master"
    },
    "travis-ci/build-configs/other.yml@v1.0.0"
  ],
  "git": {
    "depth": 3,
    "submodules": true,
    "clone": false,
    "lfs_skip_smudge": false,
    "quiet": false,
    "sparse_checkout": "skip-worktree-map-file",
    "autocrlf": "input"
  },
  "env": {
    "global": [
      "CAMPFIRE_TOKEN=abc123",
      "TIMEOUT=1000",
      {
        "secure": "wD4B2XrAYXFIq1PAEic="
      }
    ],
    "jobs": [
      "USE_NETWORK=true",
      "USE_NETWORK=false",
      {
        "secure": "wD4B2XrAYXFIq1PAEic="
      }
    ]
  }
}
