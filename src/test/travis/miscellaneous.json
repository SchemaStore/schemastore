{
  "version": "~> 1.0.0",
  "import": [
    {
      "source": ".travis/other.yml",
      "mode": "deep_merge",
      "if": "branch != master"
    },
    "travis-ci/build-configs/other.yml@v1.0.0"
  ],
  "git": {
    "depth": 3,
    "submodules": true,
    "clone": false,
    "lfs_skip_smudge": false,
    "quiet": false,
    "sparse_checkout": "skip-worktree-map-file",
    "autocrlf": "input"
  }
}
