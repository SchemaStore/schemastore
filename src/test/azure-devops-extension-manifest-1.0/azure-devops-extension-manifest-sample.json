{
  "branding": {
    "color": "rgb(34, 34, 34)",
    "theme": "dark"
  },
  "categories": ["Azure Boards"],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "eula.md"
    }
  },
  "contributions": [
    {
      "description": "Adds a menu action from builds grid to show associated items.",
      "id": "showCommits",
      "properties": {
        "title": "View associated items",
        "uri": "launch.html"
      },
      "targets": ["ms.vss-build-web.completed-build-menu"],
      "type": "ms.vss-web.action"
    }
  ],
  "description": "Awesome tools to help you and your team do great things everyday.",
  "files": [
    {
      "addressable": true,
      "path": "launch.html"
    },
    {
      "addressable": true,
      "packagePath": "lib",
      "path": "node_modules/vss-web-extension-sdk/lib"
    }
  ],
  "icons": {
    "default": "images/fabrikam-logo.png"
  },
  "id": "tools",
  "links": {
    "getstarted": {
      "uri": "https://www.fabrikam-fiber-inc.com/help/getstarted"
    },
    "support": {
      "uri": "https://www.fabrikam-fiber-inc.com/support"
    }
  },
  "manifestVersion": 1,
  "name": "Fabrikam Tools",
  "publisher": "fabrikam",
  "repository": {
    "type": "git",
    "uri": "https://github.com/fabrikam-fiber-inc/myextension"
  },
  "scopes": ["vso.work", "vso.code_write", "vso.build_execute"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "version": "0.1.0"
}
