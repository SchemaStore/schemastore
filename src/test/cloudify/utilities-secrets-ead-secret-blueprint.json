{
  "tosca_definitions_version": "cloudify_dsl_1_3",
  "imports": [
    "http://www.getcloudify.org/spec/cloudify/5.0.0/types.yaml",
    "plugin:cloudify-utilities-plugin"
  ],
  "inputs": {
    "variant": {
      "default": "lab1_tenantA"
    }
  },
  "node_types": {
    "cloudify.openstack.nodes.ImportantOpenstackResourceMock": {
      "derived_from": "cloudify.nodes.Root",
      "properties": {
        "important_openstack_resource_mock": {
          "default": {}
        },
        "openstack_config": {
          "default": {}
        }
      }
    }
  },
  "node_templates": {
    "openstack_config_provider": {
      "type": "cloudify.nodes.secrets.Reader",
      "properties": {
        "keys": [
          "openstack_config"
        ],
        "variant": {
          "get_input": "variant"
        }
      }
    },
    "resource_config_data_provider": {
      "type": "cloudify.nodes.secrets.Reader",
      "properties": {
        "keys": [
          "some_key_1",
          "some_key_4",
          "some_key_7",
          "some_key_10"
        ]
      }
    },
    "important_resource": {
      "type": "cloudify.openstack.nodes.ImportantOpenstackResourceMock",
      "properties": {
        "important_openstack_resource_mock": {
          "resource_config_key_1": {
            "get_attribute": [
              "resource_config_data_provider",
              "data",
              "some_key_1",
              "value"
            ]
          },
          "resource_config_key_4": {
            "get_attribute": [
              "resource_config_data_provider",
              "data",
              "some_key_4",
              "value"
            ]
          },
          "resource_config_key_7": {
            "get_attribute": [
              "resource_config_data_provider",
              "data",
              "some_key_7",
              "value"
            ]
          }
        },
        "openstack_config": {
          "get_attribute": [
            "openstack_config_provider",
            "data",
            "openstack_config",
            "value"
          ]
        }
      },
      "relationships": [
        {
          "type": "cloudify.relationships.depends_on",
          "target": "openstack_config_provider"
        },
        {
          "type": "cloudify.relationships.depends_on",
          "target": "resource_config_data_provider"
        }
      ]
    }
  },
  "outputs": {
    "resource_config_key_1": {
      "value": {
        "get_attribute": [
          "resource_config_data_provider",
          "data",
          "some_key_1",
          "value"
        ]
      }
    },
    "resource_config_key_4": {
      "value": {
        "get_attribute": [
          "resource_config_data_provider",
          "data",
          "some_key_4",
          "value"
        ]
      }
    },
    "resource_config_key_7": {
      "value": {
        "get_attribute": [
          "resource_config_data_provider",
          "data",
          "some_key_7",
          "value"
        ]
      }
    },
    "resource_config_key_10": {
      "value": {
        "get_attribute": [
          "resource_config_data_provider",
          "data",
          "some_key_10",
          "value"
        ]
      }
    },
    "openstack_config": {
      "value": {
        "get_attribute": [
          "openstack_config_provider",
          "data",
          "openstack_config",
          "value"
        ]
      }
    },
    "openstack_username": {
      "value": {
        "get_attribute": [
          "openstack_config_provider",
          "data",
          "openstack_config",
          "value",
          "username"
        ]
      }
    }
  }
}