{
  "$schema": "../../schemas/json/package.json",
  "name": "pnpm-fields",
  "pnpm": {
    "allowNonAppliedPatches": true,
    "allowedDeprecatedVersions": {
      "express": "1"
    },
    "auditConfig": {
      "ignoreCves": ["CVE-2022-36313"],
      "ignoreGhsas": ["GHSA-42xw-2xvc-qx8m"]
    },
    "executionEnv": {
      "nodeVersion": "22"
    },
    "ignoredOptionalDependencies": ["fsevents"],
    "neverBuiltDependencies": ["fsevents"],
    "onlyBuiltDependencies": ["level"],
    "onlyBuiltDependenciesFile": "node_modules/@my-org/policy/onlyBuiltDependencies.json",
    "overrides": {
      "bar@^2.1.0": "3.0.0",
      "foo": "^1.0.0",
      "qar@1>zoo": "2",
      "quux": "npm:@myorg/quux@^1.0.0"
    },
    "packageExtensions": {
      "express@1": {
        "optionalDependencies": {
          "typescript": "2"
        }
      },
      "fork-ts-checker-webpack-plugin": {
        "dependencies": {
          "@babel/core": "1"
        },
        "peerDependencies": {
          "eslint": ">= 6"
        },
        "peerDependenciesMeta": {
          "eslint": {
            "optional": true
          }
        }
      }
    },
    "patchedDependencies": {
      "express@4.18.1": "patches/express@4.18.1.patch"
    },
    "peerDependencyRules": {
      "allowAny": ["eslint"],
      "allowedVersions": {
        "button@2>react": "17",
        "card": "17"
      },
      "ignoreMissing": ["@babel/*", "react"]
    },
    "requiredScripts": ["build"],
    "supportedArchitectures": {
      "cpu": ["x64"],
      "libc": ["musl"],
      "os": ["darwin", "linux"]
    },
    "updateConfig": {
      "ignoreDependencies": ["load-json-file"]
    }
  }
}
