{
  "$schema": "http://localhost:8000/rc3-request-0.0.3.json",
  "auth": {
    "password": "is-cool",
    "type": "basic",
    "username": "gary"
  },
  "body": {
    "json": {
      "any": "json/object"
    },
    "text": "Hello World"
  },
  "comment": "no comment",
  "extract": {
    "env": "global",
    "json_path": "$.path.in.json[2]",
    "text_pattern": "regex with one (group.*) to extract",
    "var": "token"
  },
  "form_data": {
    "key1": "value1",
    "key2": "value2"
  },
  "headers": {
    "x-ff": "value2",
    "x-trace-id": "value1"
  },
  "method": "GET",
  "params": {
    "key1": "value1",
    "key2": "value2"
  },
  "url": "http://localhost:8080/v1/greetings"
}
