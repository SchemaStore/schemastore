{
  "environments": {
    "production": {
      "configuration": {
        "deployment": {
          "strategy": "blue-green",
          "timeout": "30m"
        },
        "monitoring": {
          "alerts": ["cpu", "memory", "errors"],
          "enabled": true
        }
      },
      "name": "Production API",
      "variables": {
        "api_url": "https://api.prod.example.com",
        "features": {
          "caching": true,
          "monitoring": {
            "enabled": true,
            "interval": "1m"
          }
        },
        "rate_limit": 1000
      }
    }
  },
  "id": "service-api",
  "workflow_dispatch": "deploy.yml"
}
