{
  "environments": {
    "production": {
      "configuration": {
        "deployment": {
          "strategy": "blue-green",
          "timeout": "1h"
        }
      }
    },
    "staging": {
      "configuration": {
        "deployment": {
          "strategy": "rolling",
          "timeout": "15m"
        }
      }
    }
  },
  "id": "service-deployment",
  "workflow_dispatch": "deploy.yml"
}
