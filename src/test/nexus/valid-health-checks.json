{
  "environments": {
    "production": {
      "configuration": {
        "health_checks": {
          "liveness": {
            "endpoint": "/health/live",
            "failure_threshold": 3,
            "initial_delay": "30s"
          },
          "readiness": {
            "endpoint": "/health/ready",
            "interval": "10s",
            "success_threshold": 2,
            "timeout": "5s"
          }
        }
      }
    }
  },
  "id": "service-api",
  "workflow_dispatch": "deploy.yml"
}
