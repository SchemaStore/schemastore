{
  "dependency": {
    "after": ["metrics"],
    "before": ["api"],
    "id": ["auth"]
  },
  "environments": {
    "production": {
      "configuration": {
        "deployment": {
          "strategy": "blue-green",
          "timeout": "30m"
        }
      },
      "variables": {
        "API_URL": "https://api.example.com"
      }
    }
  },
  "id": "service-complete",
  "job_name_prefix": "complete",
  "variables": {
    "config": "{\"feature\": true}",
    "version": "1.0.0"
  },
  "workflow_dispatch": "deploy.yml"
}
