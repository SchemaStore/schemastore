{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.1.1",
      "destination": "js/lib",
      "files": [ "jquery.js", "jquery.min.js" ]
    },
    {
      "library": "knockout@3.4.1",
      "destination": "js/lib",
      "files": [ "knockout-min.js" ]
    },
    {
      "provider": "filesystem",
      "library": "protocol:fullpath/to/resource.js",
      "destination": "js/lib",
      "files": [ "knockout-min.js" ]
    }
  ]
}
