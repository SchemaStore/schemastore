{
  "name": "typst-lsp",
  "description": "A brand-new language server for Typst.",
  "homepage": "https://github.com/nvarner/typst-lsp",
  "licenses": [
    "MIT"
  ],
  "languages": [
    "Typst"
  ],
  "categories": [
    "LSP"
  ],
  "source": {
    "id": "pkg:github/nvarner/typst-lsp@v0.3.2",
    "asset": [
      {
        "target": "darwin_arm64",
        "file": "typst-lsp-darwin-arm64"
      },
      {
        "target": "darwin_x64",
        "file": "typst-lsp-darwin-x64"
      },
      {
        "target": "linux_arm64",
        "file": "typst-lsp-linux-arm64"
      },
      {
        "target": "linux_x64",
        "file": "typst-lsp-linux-x64"
      },
      {
        "target": "win_arm64",
        "file": "typst-lsp-win32-arm64.exe"
      },
      {
        "target": "win_x64",
        "file": "typst-lsp-win32-x64.exe"
      }
    ]
  },
  "bin": {
    "typst-lsp": "{{source.asset.file}}"
  }
}
