{
  "elements": [
    {
      "name": "persistentHeader",
      "selector": {
        "css": "header"
      },
      "elements": [
        {
          "name": "menu",
          "selector": {
            "css": ".menu"
          },
          "elements": [
            {
              "public": true,
              "name": "grammarMenuItem",
              "selector": {
                "css": "[data-navbar-id='grammar']"
              },
              "type": "clickable"
            },
            {
              "public": true,
              "type": "clickable",
              "name": "menuItems",
              "selector": {
                "css": "li",
                "returnAll": true
              }
            }
          ]
        }
      ]
    },
    {
      "name": "content",
      "public": true,
      "selector": {
        "css": "main.content"
      }
    }
  ],
  "root": true,
  "selector": {
    "css": "body"
  }
}
