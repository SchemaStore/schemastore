{
  "$schema": "https://json.schemastore.org/docfx",
  "build": {
    "content": [
      {
        "dest": "core/api",
        "files": ["*.yml"],
        "src": "api_ref"
      },
      {
        "dest": "articles",
        "exclude": ["**/includes/**", "***/contributing.md"],
        "files": ["**/*.md"],
        "src": "docs"
      },
      {
        "dest": "core/api",
        "files": ["*.md"],
        "src": "api"
      },
      {
        "files": ["toc.yml", "index.md"]
      }
    ],
    "dest": "_site",
    "externalReference": [],
    "globalMetadata": {
      "_displayLangs": ["csharp"],
      "author": "dotnet-bot",
      "breadcrumb_path": "/dotnet/toc.json"
    },
    "overwrite": "apidoc/*.md",
    "resource": [
      {
        "dest": "articles",
        "exclude": ["**/obj/**", "_themes/DocPacker/**"],
        "files": [
          "images/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "src": "docs"
      }
    ],
    "template": ["docs.html"],
    "xref": [
      "_zip/msdn.4.5.2.zip",
      "_zip/namespaces.4.5.2.zip",
      "_zip/vs.110.zip",
      "_zip/vs.140.zip",
      "_zip/office15.zip",
      "_zip/missingapi.yml"
    ]
  },
  "metadata": [
    {
      "dest": "api",
      "src": [
        {
          "cwd": "..",
          "exclude": ["**/bin/**", "**/obj/**"],
          "files": [
            "corefx/src/**/ref/**/*.csproj",
            "wcf/src/**/ref/**/*.csproj"
          ]
        }
      ]
    },
    {
      "dest": "api_src",
      "src": [
        {
          "cwd": "..",
          "exclude": ["**/bin/**", "**/obj/**"],
          "files": [
            "corefx/src/**/src/**/*.csproj",
            "wcf/src/**/src/**/*.csproj",
            "coreclr/src/mscorlib/System.Private.CoreLib.csproj"
          ]
        }
      ]
    }
  ]
}
