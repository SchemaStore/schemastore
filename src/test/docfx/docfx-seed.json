{
  "$schema": "https://json.schemastore.org/docfx",
  "build": {
    "content": [
      {
        "cwd": "obj/api",
        "dest": "api",
        "files": ["**/*.yml"]
      },
      {
        "files": ["articles/**/*.md", "*.md", "toc.yml", "restapi/**"]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "docfx seed website",
      "_enableSearch": true
    },
    "overwrite": "specs/*.md",
    "resource": [
      {
        "files": ["articles/images/**"]
      }
    ]
  },
  "metadata": [
    {
      "dest": "obj/api",
      "src": [
        {
          "cwd": "src",
          "exclude": ["**/bin/**", "**/obj/**"],
          "files": ["**/*.sln"]
        }
      ]
    }
  ]
}
