{
  "$schema": "https://json.schemastore.org/docfx",
  "build": {
    "content": [
      {
        "exclude": [
          "**/obj/**",
          "aspnet/**",
          "**/includes/**",
          "**/aspnet-core-conceptual/**"
        ],
        "files": ["**/*.md"]
      }
    ],
    "dest": "_site",
    "externalReference": [],
    "fileMetadata": {},
    "globalMetadata": {
      "_navPath": "/foo",
      "_navRel": "/foo",
      "breadcrumb_path": "/aspnet/breadcrumb/toc.json"
    },
    "overwrite": [],
    "resource": [
      {
        "exclude": [
          "**/obj/**",
          "aspnet/core/**",
          "**/includes/**",
          "_site/**"
        ],
        "files": ["**/*.png", "**/*.jpg"]
      }
    ],
    "template": []
  }
}
