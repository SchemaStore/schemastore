{
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "32": "icons/icon-32.png"
    },
    "default_popup": "popup.html",
    "default_title": "Navigation Stats"
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "50.0"
    }
  },
  "icons": {
    "32": "icons/icon-32.png"
  },
  "manifest_version": 2,
  "name": "Navigation Stats",
  "permissions": ["webNavigation", "storage"],
  "version": "0.1.0"
}
