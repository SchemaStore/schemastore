{
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "60.0a1"
    }
  },
  "description": "Adds a context menu item that labels links with the hostname. Demo of onShown and refresh().",
  "icons": {
    "16": "icon/label.svg",
    "32": "icon/label.svg",
    "48": "icon/label.svg"
  },
  "manifest_version": 2,
  "name": "Labelled open",
  "permissions": ["menus", "<all_urls>"],
  "version": "1.0"
}
