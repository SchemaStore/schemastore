{
  "$schema": "https://json.schemastore.org/boyka-config",
  "listeners_package": "io.github.boykaframework.testng.listeners",
  "ui": {
    "logging": {
      "exclude_logs": ["bugreport"]
    },
    "mobile": {
      "test_bs_ios": {
        "device": {
          "application": {
            "external": true,
            "install_timeout": 180,
            "path": "IOSApp"
          },
          "capabilities": {
            "appiumLogs": true,
            "appiumVersion": "2.0.0",
            "automationVersion": "latest",
            "buildName": "Test BrowserStack Build",
            "debug": true,
            "deviceLogs": true,
            "networkLogs": true,
            "projectName": "BrowserStack iOS Project",
            "sessionName": "Test BrowserStack Session",
            "video": true
          },
          "name": "iPhone 14",
          "os": "IOS",
          "permissions": {
            "MICROPHONE": "YES",
            "NOTIFICATIONS": "YES"
          },
          "swipe": {
            "max_swipe_until_found": 5
          },
          "type": "CLOUD",
          "version": "16"
        },
        "server": {
          "base_path": "/wd/hub",
          "driver": "XCUI",
          "password": "${env:BS_KEY}",
          "target": "BROWSER_STACK",
          "user_name": "${env:BS_USER}"
        }
      },
      "test_grid_wdio_ios": {
        "device": {
          "application": {
            "install_timeout": 180,
            "path": "/apps/ios/wdio-demo.app.zip",
            "type": "HYBRID"
          },
          "connect_keyboard": false,
          "name": "iPhone 14",
          "os": "IOS",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "type": "VIRTUAL",
          "typing_speed": 35,
          "version": "16.2",
          "virtual_device": {
            "headless": true,
            "launch_timeout": 180
          },
          "wda": {
            "connection_timeout": 120,
            "launch_timeout": 120
          }
        },
        "server": {
          "driver": "XCUI",
          "external": true,
          "port": 4444,
          "target": "LOCAL"
        }
      },
      "test_local_ios_web": {
        "device": {
          "application": {
            "base_url": "https://the-internet.herokuapp.com/",
            "browser": "SAFARI",
            "type": "WEB"
          },
          "connect_keyboard": false,
          "name": "iPhone 14",
          "os": "IOS",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "type": "VIRTUAL",
          "typing_speed": 35,
          "version": "16.2",
          "video": {
            "enabled": true,
            "ios": {
              "fps": 30
            },
            "time_limit": 300
          },
          "virtual_device": {
            "headless": true,
            "launch_timeout": 180
          },
          "wda": {
            "connection_timeout": 120,
            "launch_timeout": 120,
            "local_port": 8102
          }
        },
        "server": {
          "driver": "XCUI",
          "port": 4727,
          "session_override": true,
          "target": "LOCAL"
        }
      },
      "test_local_wdio_ios": {
        "device": {
          "application": {
            "install_timeout": 180,
            "path": "/apps/ios/wdio-demo.app.zip",
            "type": "HYBRID"
          },
          "connect_keyboard": false,
          "name": "iPhone 14",
          "os": "IOS",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "type": "VIRTUAL",
          "typing_speed": 35,
          "version": "16.2",
          "virtual_device": {
            "headless": true,
            "launch_timeout": 180
          },
          "wda": {
            "connection_timeout": 120,
            "launch_timeout": 120,
            "local_port": 8101
          }
        },
        "server": {
          "callback_port": 4726,
          "driver": "XCUI",
          "logging": {
            "debug_spacing": true,
            "local_timezone": true,
            "timestamp": true
          },
          "other_args": {
            "--callback-address": "127.0.0.1",
            "--callback-port": 4726,
            "--long-stacktrace": true
          },
          "port": 4726,
          "session_override": true,
          "target": "LOCAL"
        }
      },
      "test_lt_ios": {
        "device": {
          "application": {
            "install_timeout": 180
          },
          "capabilities": {
            "app": "IOSApp",
            "autoAcceptAlerts": true,
            "autoGrantPermissions": true,
            "build": "Test LambdaTest Build",
            "deviceName": "iPhone 14",
            "devicelog": true,
            "isRealMobile": true,
            "name": "Test LambdaTest Session",
            "network": true,
            "platformName": "iOS",
            "platformVersion": "16",
            "project": "LambdaTest iOS Project",
            "video": true,
            "visual": true,
            "w3c": true
          },
          "swipe": {
            "max_swipe_until_found": 5
          },
          "type": "CLOUD"
        },
        "server": {
          "base_path": "/wd/hub",
          "driver": "XCUI",
          "password": "${env:LT_KEY}",
          "target": "LAMBDA_TEST_MOBILE",
          "user_name": "${env:LT_USER}"
        }
      }
    },
    "screenshot": {
      "enabled": true,
      "extension": "jpeg",
      "path": "./screenshots",
      "prefix": "SCR"
    },
    "timeout": {
      "explicit_wait": 30,
      "highlight_delay": 100,
      "implicit_wait": 10,
      "page_load_timeout": 30,
      "script_timeout": 10
    }
  }
}
