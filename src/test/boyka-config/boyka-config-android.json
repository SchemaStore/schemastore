{
  "$schema": "https://json.schemastore.org/boyka-config",
  "listeners_package": "io.github.boykaframework.testng.listeners",
  "ui": {
    "logging": {
      "exclude_logs": ["bugreport"]
    },
    "mobile": {
      "test_bs_android": {
        "device": {
          "application": {
            "external": true,
            "install_timeout": 180,
            "path": "AndroidApp",
            "wait_activity": "com.swaglabsmobileapp.MainActivity"
          },
          "capabilities": {
            "appiumLogs": true,
            "appiumVersion": "2.0.0",
            "automationVersion": "latest",
            "buildName": "Test BrowserStack Build",
            "debug": true,
            "deviceLogs": true,
            "networkLogs": true,
            "projectName": "BrowserStack Android Project",
            "sessionName": "Test BrowserStack Session",
            "video": true
          },
          "ignore_unimportant_views": true,
          "name": "Google Pixel 5",
          "os": "ANDROID",
          "swipe": {
            "max_swipe_until_found": 5
          },
          "type": "CLOUD",
          "version": "12.0"
        },
        "server": {
          "base_path": "/wd/hub",
          "driver": "UI_AUTOMATOR",
          "password": "${env:BS_KEY}",
          "target": "BROWSER_STACK",
          "user_name": "${env:BS_USER}"
        }
      },
      "test_grid_wdio_android": {
        "device": {
          "application": {
            "install_timeout": 180,
            "path": "/apps/android/wdio-demo.apk",
            "type": "HYBRID",
            "wait_timeout": 120
          },
          "full_reset": true,
          "ignore_unimportant_views": true,
          "name": "Pixel_8_Pro",
          "os": "ANDROID",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "type": "VIRTUAL",
          "version": "11",
          "virtual_device": {
            "headless": true,
            "name": "Pixel_8_Pro"
          }
        },
        "server": {
          "driver": "UI_AUTOMATOR",
          "external": true,
          "port": 4444,
          "target": "LOCAL"
        }
      },
      "test_local_android_web": {
        "device": {
          "adb_timeout": 120,
          "application": {
            "base_url": "https://the-internet.herokuapp.com/",
            "browser": "CHROME",
            "type": "WEB"
          },
          "name": "Pixel_8_Pro",
          "no_reset": true,
          "os": "ANDROID",
          "server_install_timeout": 120,
          "server_launch_timeout": 120,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "system_port": 8202,
          "type": "VIRTUAL",
          "version": "11",
          "video": {
            "android": {
              "bit_rate": 4
            },
            "enabled": true,
            "size": "800x720",
            "time_limit": 300
          },
          "virtual_device": {
            "headless": true,
            "name": "Pixel_8_Pro"
          }
        },
        "server": {
          "allow_insecure": ["get_server_logs", "chromedriver_autodownload"],
          "driver": "UI_AUTOMATOR",
          "port": 4728,
          "session_override": true,
          "target": "LOCAL"
        }
      },
      "test_local_wdio_android": {
        "device": {
          "application": {
            "install_timeout": 180,
            "path": "/apps/android/wdio-demo.apk",
            "type": "HYBRID",
            "wait_timeout": 120
          },
          "full_reset": true,
          "ignore_unimportant_views": true,
          "name": "Pixel_8_Pro",
          "os": "ANDROID",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "system_port": 8201,
          "type": "VIRTUAL",
          "version": "11",
          "video": {
            "android": {
              "bit_rate": 4
            },
            "enabled": true,
            "size": "800x720",
            "time_limit": 300
          },
          "virtual_device": {
            "headless": true,
            "name": "Pixel_8_Pro"
          }
        },
        "server": {
          "driver": "UI_AUTOMATOR",
          "external": true,
          "port": 4724,
          "target": "LOCAL"
        }
      },
      "test_lt_android": {
        "device": {
          "application": {
            "install_timeout": 180,
            "wait_activity": "com.swaglabsmobileapp.MainActivity"
          },
          "auto_grant_permissions": true,
          "capabilities": {
            "app": "AndroidApp",
            "autoAcceptAlerts": true,
            "autoGrantPermissions": true,
            "build": "Test LambdaTest Build",
            "deviceName": "Pixel 5",
            "devicelog": true,
            "isRealMobile": true,
            "name": "Test LambdaTest Session",
            "network": true,
            "platformName": "Android",
            "platformVersion": "11",
            "project": "LambdaTest Android Project",
            "video": true,
            "visual": true,
            "w3c": true
          },
          "ignore_unimportant_views": true,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "type": "CLOUD"
        },
        "server": {
          "base_path": "/wd/hub",
          "driver": "UI_AUTOMATOR",
          "password": "${env:LT_KEY}",
          "target": "LAMBDA_TEST_MOBILE",
          "user_name": "${env:LT_USER}"
        }
      }
    },
    "screenshot": {
      "enabled": true,
      "extension": "jpeg",
      "path": "./screenshots",
      "prefix": "SCR"
    },
    "timeout": {
      "explicit_wait": 30,
      "highlight_delay": 100,
      "implicit_wait": 10,
      "page_load_timeout": 30,
      "script_timeout": 10
    }
  }
}
