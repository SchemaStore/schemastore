{
  "entrypoints": [
    { "id": "do-something", "label": "Do something!", "type": "command" },
    {
      "icons": [
        {
          "height": 24,
          "path": "assets/icons/my-icon.png",
          "scale": [1, 2],
          "species": ["toolbar"],
          "theme": ["lightest", "darkest"],
          "width": 24
        }
      ],
      "id": "nice-panel",
      "label": { "default": "Nice panel", "es": "Buen panel" },
      "type": "panel"
    }
  ],
  "featureFlags": {
    "enableFillAsCustomAttribute": true,
    "enableSWCSupport": true
  },
  "host": [
    {
      "app": "ID",
      "maxVersion": "24.0",
      "minVersion": "23.7.1"
    },
    {
      "app": "PS",
      "data": {
        "apiVersion": 1,
        "enableMenuRecording": false,
        "loadEvent": "use"
      },
      "minVersion": "19.1"
    }
  ],
  "icons": [
    {
      "height": 24,
      "path": "assets/icons/plugin-icon.png",
      "scale": [1, 1.25, 1.5, 1.75, 2],
      "species": ["pluginList"],
      "theme": ["lightest", "darkest"],
      "width": 24
    }
  ],
  "id": "3cc45790",
  "main": "index.html",
  "manifestVersion": 5,
  "name": "My Plug-in",
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": false,
    "clipboard": "readAndWrite",
    "enableUserInfo": true,
    "ipc": {
      "enablePluginCommunication": true
    },
    "launchProcess": {
      "extensions": ["pdf", "png", "jpg"],
      "schemes": ["http", "https"]
    },
    "localFileSystem": "fullAccess",
    "network": { "domains": ["https://*.adobe.com"] },
    "webview": {
      "allow": "yes",
      "domains": ["https://*.adobe.com", "https://*.google.com"]
    }
  },
  "version": "1.0.0"
}
