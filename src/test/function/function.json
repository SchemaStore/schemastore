{
  "bindings": [
    {
      "authLevel": "anonymous",
      "direction": "in",
      "methods": ["get", "post"],
      "name": "req",
      "type": "httpTrigger"
    },
    {
      "direction": "out",
      "name": "res",
      "type": "http"
    }
  ],
  "retry": {
    "Strategy": "exponentialBackoff",
    "maxRetryCount": 2,
    "maximumInterval": "00:03:00",
    "minimumInterval": "00:00:10"
  }
}
