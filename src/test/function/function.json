{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "retry": {
    "Strategy": "exponentialBackoff",
    "maxRetryCount": 2,
    "minimumInterval": "00:00:10",
    "maximumInterval": "00:03:00"
  }
}
