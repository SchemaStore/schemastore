{
  "minify": {
    "disableCSS": false,
    "disableHTML": false,
    "disableJS": false,
    "disableJSON": false,
    "disableSVG": false,
    "disableXML": false,
    "minifyOutput": false,
    "tdewolff": {
      "css": {
        "keepCSS2": true,
        "precision": 3
      },
      "html": {
        "keepComments": false,
        "keepConditionalComments": true,
        "keepDefaultAttrVals": true,
        "keepDocumentTags": true,
        "keepEndTags": true,
        "keepQuotes": false,
        "keepWhitespace": false
      },
      "json": {
        "keepNumbers": false,
        "precision": 0
      },
      "svg": {
        "precision": 0
      },
      "xml": {
        "keepWhitespace": false
      }
    }
  }
}
