{
  "data_dir": "/var/lib/vector/",
  "api": {
    "enabled": false,
    "address": "127.0.0.1:8686",
    "playground": true
  },
  "schema": {
    "enabled": false,
    "validation": false
  },
  "healthchecks": {
    "enabled": true,
    "require_healthy": false
  },
  "sources": {
    "generate_syslog": {
      "type": "demo_logs",
      "interval": 1.0,
      "count": 100,
      "format": "syslog",
      "framing": {
        "method": "bytes"
      },
      "decoding": {
        "codec": "bytes"
      }
    }
  },
  "sinks": {
    "emit_syslog": {
      "inputs": [
        "remap_syslog"
      ],
      "healthcheck": {
        "enabled": true
      },
      "type": "console",
      "target": "stdout",
      "encoding": {
        "codec": "json",
        "json": {
          "pretty": false
        }
      }
    }
  },
  "transforms": {
    "remap_syslog": {
      "inputs": [
        "generate_syslog"
      ],
      "type": "remap",
      "source": "structured = parse_syslog!(.message)\n. = merge(., structured)\n",
      "metric_tag_values": "single",
      "drop_on_error": false,
      "drop_on_abort": true,
      "reroute_dropped": false,
      "runtime": "ast"
    }
  },
  "enrichment_tables": {},
  "tests": [],
  "secret": {}
}