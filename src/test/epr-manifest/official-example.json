{
  "$schema": "https://json.schemastore.org/epr-manifest",
  "defaultNavBehavior": "block",
  "defaultResBehavior": "block",
  "maxAge": 31536000,
  "reportUrl": "https://example.com",
  "rules": [
    {
      "allowData": false,
      "path": "/",
      "types": ["navigation"]
    },
    {
      "allowData": false,
      "regex": "^/\\d+$",
      "types": ["navigation"]
    },
    {
      "allowData": true,
      "path": "/image",
      "types": ["image"]
    },
    {
      "allowData": false,
      "regex": "^/(scoreboard|random|favorites|recentvisits|create)$",
      "types": ["navigation"]
    },
    {
      "allowData": true,
      "regex": "^/(recent|popular|metrics|template|search)$",
      "types": ["navigation"]
    }
  ],
  "site": "https://example.com"
}
