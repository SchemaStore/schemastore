{
  "bindings": [
    {
      "context": "Global",
      "bindings": {
        "ctrl+c": "app:interrupt",
        "ctrl+d": "app:exit"
      }
    },
    {
      "context": "Chat",
      "bindings": {
        "enter": "chat:submit",
        "ctrl+e": "chat:externalEditor",
        "ctrl+z": "chat:undo"
      }
    }
  ]
}
