{
  "bindings": [
    {
      "context": "Global",
      "bindings": {
        "ctrl+c": "app:interrupt"
      }
    },
    {
      "context": "Chat",
      "bindings": {
        "enter": "chat:submit"
      }
    },
    {
      "context": "Autocomplete",
      "bindings": {
        "tab": "autocomplete:accept"
      }
    },
    {
      "context": "Confirmation",
      "bindings": {
        "y": "confirm:yes"
      }
    },
    {
      "context": "Help",
      "bindings": {
        "escape": "help:dismiss"
      }
    },
    {
      "context": "Transcript",
      "bindings": {
        "escape": "transcript:exit"
      }
    },
    {
      "context": "HistorySearch",
      "bindings": {
        "escape": "historySearch:cancel"
      }
    },
    {
      "context": "Task",
      "bindings": {
        "ctrl+z": "task:background"
      }
    },
    {
      "context": "ThemePicker",
      "bindings": {
        "s": "theme:toggleSyntaxHighlighting"
      }
    },
    {
      "context": "Settings",
      "bindings": {
        "/": "settings:search"
      }
    },
    {
      "context": "Tabs",
      "bindings": {
        "tab": "tabs:next"
      }
    },
    {
      "context": "Attachments",
      "bindings": {
        "backspace": "attachments:remove"
      }
    },
    {
      "context": "Footer",
      "bindings": {
        "enter": "footer:openSelected"
      }
    },
    {
      "context": "MessageSelector",
      "bindings": {
        "up": "messageSelector:up"
      }
    },
    {
      "context": "DiffDialog",
      "bindings": {
        "escape": "diff:dismiss"
      }
    },
    {
      "context": "ModelPicker",
      "bindings": {
        "left": "modelPicker:decreaseEffort"
      }
    },
    {
      "context": "Select",
      "bindings": {
        "enter": "select:accept"
      }
    },
    {
      "context": "Plugin",
      "bindings": {
        "i": "plugin:install"
      }
    }
  ]
}
