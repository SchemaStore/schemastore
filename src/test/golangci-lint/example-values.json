  "linters-settings": {
    "asasalint": {
      "exclude": ["Append", "\\.Wrapf"],
      "ignore-test": true,
      "use-builtin-exclusions": false
    },
    "bidichk": {
      "first-strong-isolate": false,
      "left-to-right-embedding": false,
      "left-to-right-isolate": false,
      "left-to-right-override": false,
      "pop-directional-formatting": false,
      "pop-directional-isolate": false,
      "right-to-left-embedding": false,
      "right-to-left-isolate": false,
      "right-to-left-override": false
    },
    "custom": {
      "example": {
        "description": "This is an example usage of a plugin linter.",
        "original-url": "github.com/golangci/example-linter",
        "path": "/path/to/example.so"
      }
    },
    "cyclop": {
      "max-complexity": 10,
      "package-average": 0.5,
      "skip-tests": true
    },
    "decorder": {
      "dec-order": ["type", "const", "var", "func"],
      "disable-const-dec-num-check": false,
      "disable-dec-num-check": false,
      "disable-dec-order-check": false,
      "disable-init-func-first-check": false,
      "disable-type-dec-num-check": false,
      "disable-var-dec-num-check": false,
      "ignore-underscore-vars": false
    },
    "depguard": {
      "rules": {
        "main": {
          "allow": ["$gostd", "github.com/OpenPeeDeeP"],
          "deny": [
            {
              "desc": "not allowed",
              "pkg": "github.com/sirupsen/logrus"
            },
            {
              "desc": "Should be replaced by standard lib errors package",
              "pkg": "github.com/pkg/errors"
            }
          ],
          "files": ["!**/*_a _file.go"],
          "list-mode": "lax"
        }
      }
    },
    "dogsled": {
      "max-blank-identifiers": 3
    },
    "dupl": {
      "threshold": 100
    },
    "dupword": {
      "ignore": ["0C0C"],
      "keywords": ["the", "and", "a"]
    },
    "errcheck": {
      "check-blank": true,
      "check-type-assertions": true,
      "disable-default-exclusions": true,
      "exclude": "/path/to/file.txt",
      "exclude-functions": [
        "io/ioutil.ReadFile",
        "io.Copy(*bytes.Buffer)",
        "io.Copy(os.Stdout)"
      ],
      "ignore": "fmt:.*,io/ioutil:^Read.*"
    },
    "errchkjson": {
      "check-error-free-encoding": true,
      "report-no-exported": false
    },
    "errorlint": {
      "asserts": false,
      "comparison": false,
      "errorf": false,
      "errorf-multi": false
    },
    "exhaustive": {
      "check": ["switch", "map"],
      "check-generated": true,
      "default-case-required": true,
      "default-signifies-exhaustive": true,
      "explicit-exhaustive-map": true,
      "explicit-exhaustive-switch": true,
      "ignore-enum-members": "Example.+",
      "ignore-enum-types": "Example.+",
      "package-scope-only": true
    },
    "exhaustivestruct": {
      "struct-patterns": ["*.Test", "example.com/package.ExampleStruct"]
    },
    "exhaustruct": {
      "exclude": [".+/cobra\\.Command$"],
      "include": [
        ".+\\.Test",
        "example\\.com/package\\.ExampleStruct[\\d]{1,2}"
      ]
    },
    "forbidigo": {
      "analyze-types": true,
      "exclude-godoc-examples": false,
      "forbid": [
        "^print.*$",
        {
          "msg": "Do not commit print statements.",
          "p": "^fmt\\.Print.*$"
        },
        "fmt\\.Print.*(# Do not commit print statements\\.)?",
        "^spew\\.(ConfigState\\.)?Dump$",
        {
          "p": "^v1.Dump$",
          "pkg": "^example.com/pkg/api/v1$"
        }
      ]
    },
    "funlen": {
      "ignore-comments": true,
      "lines": -1,
      "statements": -1
    },
    "gci": {
      "custom-order": true,
      "local-prefixes": "github.com/org/project",
      "sections": [
        "standard",
        "default",
        "prefix(github.com/org/project)",
        "blank",
        "dot",
        "alias"
      ],
      "skip-generated": false
    },
    "ginkgolinter": {
      "allow-havelen-zero": true,
      "forbid-focus-container": true,
      "suppress-async-assertion": true,
      "suppress-compare-assertion": true,
      "suppress-err-assertion": true,
      "suppress-len-assertion": true,
      "suppress-nil-assertion": true,
      "suppress-type-compare-assertion": true
    },
    "gocognit": {
      "min-complexity": 10
    },
    "goconst": {
      "ignore-calls": false,
      "ignore-strings": "foo.+",
      "ignore-tests": true,
      "match-constant": false,
      "max": 2,
      "min": 2,
      "min-len": 2,
      "min-occurrences": 2,
      "numbers": true
    },
    "gocritic": {
      "disabled-checks": ["regexpMust"],
      "disabled-tags": [
        "diagnostic",
        "style",
        "performance",
        "experimental",
        "opinionated"
      ],
      "enabled-checks": [
        "nestingReduce",
        "unnamedResult",
        "ruleguard",
        "truncateCmp"
      ],
      "enabled-tags": [
        "diagnostic",
        "style",
        "performance",
        "experimental",
        "opinionated"
      ],
      "settings": {
        "captLocal": {
          "paramsOnly": false
        },
        "commentedOutCode": {
          "minLength": 50
        },
        "elseif": {
          "skipBalanced": false
        },
        "hugeParam": {
          "sizeThreshold": 70
        },
        "ifElseChain": {
          "minThreshold": 4
        },
        "nestingReduce": {
          "bodyWidth": 4
        },
        "rangeExprCopy": {
          "sizeThreshold": 516,
          "skipTestFuncs": false
        },
        "rangeValCopy": {
          "sizeThreshold": 32,
          "skipTestFuncs": false
        },
        "ruleguard": {
          "debug": "emptyDecl",
          "disable": "myGroupName,#myTagName",
          "enable": "myGroupName,#myTagName",
          "failOn": "dsl",
          "failOnError": false,
          "rules": "${configDir}/ruleguard/rules-*.go,${configDir}/myrule1.go"
        },
        "tooManyResultsChecker": {
          "maxResults": 10
        },
        "truncateCmp": {
          "skipArchDependent": false
        },
        "underef": {
          "skipRecvDeref": false
        },
        "unnamedResult": {
          "checkExported": true
        }
      }
    },
    "gocyclo": {
      "min-complexity": 10
    },
    "godot": {
      "capital": true,
      "exclude": ["^fixme:", "^todo:"],
      "period": false,
      "scope": "toplevel"
    },
    "godox": {
      "keywords": ["NOTE", "OPTIMIZE", "HACK"]
    },
    "gofmt": {
      "rewrite-rules": [
        {
          "pattern": "interface{}",
          "replacement": "any"
        },
        {
          "pattern": "a[b:len(a)]",
          "replacement": "a[b:]"
        }
      ],
      "simplify": false
    },
    "gofumpt": {
      "extra-rules": true,
      "lang-version": "1.17",
      "module-path": "github.com/org/project"
    },
    "goheader": {
      "template": "# Put here copyright header template for source code files\n# For example:\n# Note: {{ YEAR }} is a builtin value that returns the year relative to the current machine time.\n#\n# {{ AUTHOR }} {{ COMPANY }} {{ YEAR }}\n# SPDX-License-Identifier: Apache-2.0\n\n# Licensed under the Apache License, Version 2.0 (the \"License\");\n# you may not use this file except in compliance with the License.\n# You may obtain a copy of the License at:\n\n#   http://www.apache.org/licenses/LICENSE-2.0\n\n# Unless required by applicable law or agreed to in writing, software\n# distributed under the License is distributed on an \"AS IS\" BASIS,\n# WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\n# See the License for the specific language governing permissions and\n# limitations under the License.",
      "values": {
        "const": {
          "COMPANY": "MY COMPANY"
        },
        "regexp": {
          "AUTHOR": ".*@mycompany\\.com"
        }
      }
    },
    "goimports": {
      "local-prefixes": "github.com/org/project"
    },
    "golint": {
      "min-confidence": 0.7
    },
    "gomnd": {
      "checks": [
        "argument",
        "case",
        "condition",
        "operation",
        "return",
        "assign"
      ],
      "ignored-files": ["magic1_.+\\.go$"],
      "ignored-functions": ["^math\\.", "^http\\.StatusText$"],
      "ignored-numbers": ["0666", "0755", "42"]
    },
    "gomoddirectives": {
      "exclude-forbidden": false,
      "replace-allow-list": ["launchpad.net/gocheck"],
      "replace-local": false,
      "retract-allow-no-explanation": false
    },
    "gomodguard": {
      "allowed": {
        "domains": ["golang.org"],
        "modules": ["gopkg.in/yaml.v2"]
      },
      "blocked": {
        "local_replace_directives": false,
        "modules": [
          {
            "github.com/uudashr/go-module": {
              "reason": "`mod` is the official go.mod parser library.",
              "recommendations": ["golang.org/x/mod"]
            }
          }
        ],
        "versions": [
          {
            "github.com/mitchellh/go-homedir": {
              "reason": "testing if blocked version constraint works.",
              "version": "< 1.1.0"
            }
          }
        ]
      }
    },
    "gosec": {
      "concurrency": 12,
      "confidence": "medium",
      "config": {
        "G101": {
          "entropy_threshold": "80.0",
          "ignore_entropy": false,
          "pattern": "(?i)example",
          "per_char_threshold": "3.0",
          "truncate": "32"
        },
        "G104": {
          "fmt": ["Fscanf"]
        },
        "G111": {
          "pattern": "custom\\.Dir\\(\\)"
        },
        "G301": "0750",
        "G302": "0600",
        "G306": "0600",
        "global": {
          "#nosec": "#my-custom-nosec",
          "audit": true,
          "nosec": true,
