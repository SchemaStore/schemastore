    },
    "varnamelen": {
      "max-distance": 6,
      "min-name-length": 2,
      "check-receiver": true,
      "check-return": true,
      "ignore-type-assert-ok": true,
      "ignore-map-index-ok": true,
      "ignore-chan-recv-ok": true,
      "ignore-names": [
