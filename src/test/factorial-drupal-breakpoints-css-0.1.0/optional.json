{
  "drupal": {
    "breakpointsPath": "breakpoints.yml",
    "themeName": "themeName"
  },
  "prettier": {
    "configPath": "./.prettierrc"
  },
  "js": {
    "enabled": true,
    "path": "./breakpoints.js",
    "type": "commonjs"
  },
  "css": {
    "enabled": true,
    "path": "./breakpoints.css",
    "element": ":root"
  },
  "options": {
    "mediaQuery": true,
    "resolution": false,
    "minWidth": false,
    "maxWidth": false
  }
}
