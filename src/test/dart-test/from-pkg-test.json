{
    "fold_stack_frames": {
      "except": [
        "shelf_test_handler",
        "stream_channel",
        "test_descriptor",
        "test_process"
      ]
    },
    "presets": {
      "terse-trace": {
        "fold_stack_frames": {
          "except": [
            "test"
          ]
        }
      }
    },
    "tags": {
      "browser": {
        "timeout": "2x",
        "presets": {
          "travis": {
            "retry": 3
          }
        }
      },
      "dart2js": {
        "add_tags": [
          "browser"
        ],
        "timeout": "2x"
      },
      "firefox": {
        "add_tags": [
          "dart2js"
        ],
        "timeout": "1h4m 30S"
      },
      "chrome": {
        "add_tags": [
          "dart2js"
        ]
      },
      "phantomjs": {
        "add_tags": [
          "dart2js"
        ]
      },
      "safari": {
        "add_tags": [
          "dart2js"
        ],
        "test_on": "mac-os"
      },
      "ie": {
        "add_tags": [
          "dart2js"
        ],
        "test_on": "windows"
      },
      "pub": {
        "timeout": "2x"
      },
      "node": {
        "timeout": "2x"
      }
    }
  }