{
  "fold_stack_frames": {
    "except": [
      "shelf_test_handler",
      "stream_channel",
      "test_descriptor",
      "test_process"
    ]
  },
  "presets": {
    "terse-trace": {
      "fold_stack_frames": {
        "except": ["test"]
      }
    }
  },
  "tags": {
    "browser": {
      "presets": {
        "travis": {
          "retry": 3
        }
      },
      "timeout": "2x"
    },
    "chrome": {
      "add_tags": ["dart2js"]
    },
    "dart2js": {
      "add_tags": ["browser"],
      "timeout": "2x"
    },
    "firefox": {
      "add_tags": ["dart2js"],
      "timeout": "1h4m 30S"
    },
    "ie": {
      "add_tags": ["dart2js"],
      "test_on": "windows"
    },
    "node": {
      "timeout": "2x"
    },
    "phantomjs": {
      "add_tags": ["dart2js"]
    },
    "pub": {
      "timeout": "2x"
    },
    "safari": {
      "add_tags": ["dart2js"],
      "test_on": "mac-os"
    }
  }
}
