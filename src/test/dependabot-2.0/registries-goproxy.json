{
  "registries": {
    "my-goproxy": {
      "password": "${{ secrets.GOPROXY_PASSWORD }}",
      "type": "goproxy-server",
      "url": "https://proxy.example.com",
      "username": "user"
    }
  },
  "updates": [
    {
      "directory": "/",
      "package-ecosystem": "gomod",
      "registries": ["my-goproxy"],
      "schedule": {
        "interval": "daily"
      }
    }
  ],
  "version": 2
}
