{
  "version": 2,
  "updates": [
    {
      "package-ecosystem": "npm",
      "directory": "/1",
      "schedule": {
        "interval": "daily"
      },
      "groups": {
        "x": {
          "dependency-type": "development"
        }
      }
    },
    {
      "package-ecosystem": "npm",
      "directory": "/2",
      "schedule": {
        "interval": "daily"
      },
      "groups": {
        "x": {
          "dependency-type": "production"
        }
      }
    }
  ]
}
