{
  "updates": [
    {
      "directory": "/",
      "ignore": [
        {
          "dependency-name": "express",
          "versions": ["4.x", "5.x"]
        },
        {
          "dependency-name": "lodash"
        },
        {
          "dependency-name": "aws-sdk",
          "update-types": ["version-update:semver-patch"]
        }
      ],
      "package-ecosystem": "npm",
      "schedule": {
        "interval": "daily"
      }
    }
  ],
  "version": 2
}
