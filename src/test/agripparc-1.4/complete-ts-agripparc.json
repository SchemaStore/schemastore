{
  "$schema": "https://json.schemastore.org/agripparc-1.4.json",

  "props": "ts",
  "children": false,
  "typescript": true,
  "tsPropsDeclaration": "type",
  "flat": true,
  "stylingModule": true,
  "importReact": false,
  "overwrite": false,
  "postCommand": "echo \"nice!\"",
  "baseDir": "./src/component",
  "destination": ".",
  "allowOutsideBase": true,
  "exportType": "named",
  "declaration": "const",
  "memo": false,

  "styling": "react-native",
  "separateIndex": true,
  "reactNative": true,
  "debug": true
}
