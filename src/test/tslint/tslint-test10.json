{
	"rules": {
		"ordered-imports": [true, {
			"import-sources-order": "case-insensitive",
			"named-imports-order": "case-insensitive"
		}],
		"space-before-function-paren": [true, {
			"anonymous": "never",
			"asyncArrow": "always",
			"constructor": "never",
			"method": "never",
			"named": "never"
		}]
	}
}
