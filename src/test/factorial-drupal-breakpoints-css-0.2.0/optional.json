{
  "css": {
    "customMedia": true,
    "customProperty": true,
    "element": ":root",
    "enabled": true,
    "path": "./breakpoints.css"
  },
  "drupal": {
    "breakpointsPath": "breakpoints.yml",
    "themeName": "themeName"
  },
  "js": {
    "enabled": true,
    "path": "./breakpoints.js",
    "type": "commonjs"
  },
  "options": {
    "maxWidth": false,
    "mediaQuery": true,
    "minWidth": false,
    "resolution": false
  },
  "prettier": {
    "configPath": "./.prettierrc"
  }
}
