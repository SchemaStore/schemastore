{
  "load": {
    "name": "DOMAIN",
    "directory": "__COMET_TEST_ROOT__/incoming/DOMAIN",
    "metadata": {
      "mode": "FILE",
      "format": "DSV",
      "multiline": false,
      "array": false,
      "withHeader": false,
      "separator": ";",
      "quote": "\"",
      "escape": "\\",
      "write": "APPEND",
      "writeMode": "APPEND"
    },
    "tables": [
      {
        "name": "User",
        "pattern": "SCHEMA-.*.dsv",
        "attributes": [
          {
            "name": "firstname",
            "type": "string",
            "array": false,
            "required": false,
            "privacy": "NONE"
          },
          {
            "name": "lastname",
            "type": "string",
            "array": false,
            "required": false,
            "privacy": "SHA1"
          },
          {
            "name": "age",
            "type": "age",
            "array": false,
            "required": false,
            "privacy": "HIDE"
          }
        ],
        "metadata": {
          "format": "DSV",
          "multiline": false,
          "array": false,
          "withHeader": true,
          "separator": ";",
          "quote": "\"",
          "escape": "\\",
          "partition": {
            "sampling": 1,
            "attributes": ["YEAR", "MONTH"]
          },
          "writeMode": "APPEND"
        },
        "comment": "Schema Comment",
        "presql": ["SQL1", "SQL2"]
      }
    ],
    "comment": "Domain Comment",
    "extensions": [".json", ".csv", ".dsv", ".psv"],
    "ack": ".ack"
  }
}
