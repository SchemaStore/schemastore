{
  "load": {
    "ack": ".ack",
    "comment": "Domain Comment",
    "directory": "__COMET_TEST_ROOT__/incoming/DOMAIN",
    "extensions": [".json", ".csv", ".dsv", ".psv"],
    "metadata": {
      "array": false,
      "escape": "\\",
      "format": "DSV",
      "mode": "FILE",
      "multiline": false,
      "quote": "\"",
      "separator": ";",
      "withHeader": false,
      "writeStrategy": { "type": "APPEND" }
    },
    "name": "DOMAIN",
    "tables": [
      {
        "attributes": [
          {
            "array": false,
            "name": "firstname",
            "privacy": "NONE",
            "required": false,
            "type": "string"
          },
          {
            "array": false,
            "name": "lastname",
            "privacy": "SHA1",
            "required": false,
            "type": "string"
          },
          {
            "array": false,
            "name": "age",
            "privacy": "HIDE",
            "required": false,
            "type": "age"
          }
        ],
        "comment": "Schema Comment",
        "metadata": {
          "array": false,
          "escape": "\\",
          "format": "DSV",
          "multiline": false,
          "partition": ["YEAR", "MONTH"],
          "quote": "\"",
          "separator": ";",
          "withHeader": true
        },
        "name": "User",
        "pattern": "SCHEMA-.*.dsv",
        "presql": ["SQL1", "SQL2"]
      }
    ]
  }
}
