{
  "load": {
    "ack": ".ack",
    "comment": "Domain Comment",
    "directory": "__COMET_TEST_ROOT__/incoming/DOMAIN",
    "extensions": [".json", ".csv", ".dsv", ".psv"],
    "metadata": {
      "array": false,
      "escape": "\\",
      "format": "DSV",
      "mode": "FILE",
      "multiline": false,
      "quote": "\"",
      "separator": ";",
      "withHeader": false,
      "write": "APPEND",
      "writeMode": "APPEND"
    },
    "name": "DOMAIN",
    "tables": [
      {
        "attributes": [
          {
            "array": false,
            "name": "firstname",
            "privacy": "NONE",
            "required": false,
            "type": "string"
          },
          {
            "array": false,
            "name": "lastname",
            "privacy": "SHA1",
            "required": false,
            "type": "string"
          },
          {
            "array": false,
            "name": "age",
            "privacy": "HIDE",
            "required": false,
            "type": "age"
          }
        ],
        "name": "User",
        "pattern": "SCHEMA-.*.dsv"
      }
    ]
  }
}
