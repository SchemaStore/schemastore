{
  "$schema": "https://json.schemastore.org/chutzpah",
  "AMDAppDirectory": "appdir",
  "AMDBaseUrl": "baseUrl",
  "CodeCoverageExcludes": [],
  "CodeCoverageIncludes": [],
  "CodeCoverageTimeout": 10000,
  "CustomTestHarnessPath": "<Path to custom test harness file>",
  "EnableCodeCoverage": null,
  "Framework": "qunit",
  "FrameworkVersion": "",
  "References": [
    {
      "IsTestFrameworkFile": true,
      "Path": "require-2.1.8.js"
    },
    {
      "Path": "script.js"
    },
    {
      "Excludes": ["*.other.js"],
      "Includes": ["*.js"]
    },
    {
      "Path": "directiveTemplate_json.html",
      "TemplateOptions": {
        "Id": "template_json",
        "Mode": "Script",
        "Type": "text/ng-template"
      }
    }
  ],
  "RootReferencePathMode": "SettingsFileDirectory",
  "Server": {
    "DefaultPort": 9888,
    "Enabled": true,
    "RootPath": "../../"
  },
  "TestFileTimeout": 50000,
  "TestHarnessDirectory": "<Path to a folder>",
  "TestHarnessLocationMode": "TestFileAdjacent",
  "TestHarnessReferenceMode": "AMD",
  "TestPattern": "<Regex test name pattern>",
  "Tests": [
    {
      "ExpandReferenceComments": true,
      "Path": "test.js"
    },
    {
      "Excludes": ["*.other.js"],
      "Includes": ["*.js"],
      "Path": "tests"
    }
  ]
}
