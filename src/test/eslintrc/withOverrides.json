{
  "rules": {
    "array-bracket-spacing": [
      "error",
      "never",
      {
        "objectsInArrays": false
      }
    ]
  },
  "extends": "aaa",
  "overrides": [
    {
      "files": "*.jsx",
      "excludedFiles": "*.config.json",
      "rules": {
        "no-console": "error"
      }
    },
    {
      "files": [
        "foo/*.js"
      ],
      "rules": {}
    },
    {
      "files": [
        "bar/*.js"
      ],
      "excludedFiles": [
        "bar/excluded.js"
      ],
      "rules": {
        "semicolon": "error",
        "unknown-rule": "error"
      }
    }
  ]
}
