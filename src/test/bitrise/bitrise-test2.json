{
  "default_step_lib_source": "https://github.com/bitrise-io/bitrise-steplib.git",
  "format_version": "1.3.1",
  "project_type": "other",
  "workflows": {
    "send-notifications": {
      "steps": [
        {
          "activate-ssh-key@4.0.3": {
            "run_if": "{{getenv \"SSH_RSA_PRIVATE_KEY\" | ne \"\"}}"
          }
        },
        {
          "git-clone@4.0.11": {}
        },
        {
          "script@1.1.5": {
            "title": "Do anything with Script step"
          }
        }
      ]
    },
    "setup": {
      "steps": [
        {
          "activate-ssh-key@4.0.3": {
            "run_if": "{{getenv \"SSH_RSA_PRIVATE_KEY\" | ne \"\"}}"
          }
        },
        {
          "git-clone@4.0.11": {}
        },
        {
          "script@1.1.5": {
            "title": "Do anything with Script step"
          }
        }
      ]
    },
    "test": {
      "before_run": ["setup"],
      "envs": [
        {
          "IS_TEST": "true"
        }
      ],
      "steps": [
        {
          "git-clone@4.0.11": {}
        }
      ]
    },
    "ci": {
      "before_run": ["test"],
      "after_run": ["send-notifications"]
    },
    "deploy": {
      "before_run": ["test"],
      "steps": [
        {
          "git-clone@4.0.11": {}
        }
      ],
      "after_run": ["send-notifications"]
    }
  }
}
