{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "basics": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "object",
          "properties": {
            "prefix": {
              "type": "string",
              "description": "e.g. Dr., Ms. or Mr."
            },
            "givenName": { "type": "string" },
            "surname": { "type": "string" },
            "middleName": {
              "type": "string",
              "description": "Can be full middle name, middle initial or multiple names"
            },
            "suffix": {
              "type": "string",
              "description": "e.g. Jr. or III"
            },
            "credentials": {
              "type": "string",
              "description": "e.g. Ph.D., M.D."
            }
          },
          "required": [ "surname", "givenName" ]
        },
        "dateOfBirth": {
          "type": "string",
          "format": "date-time"
        },
        "placeOfBirth": {
          "type": "string",
          "description": "The location of your birth. This could a country or include city and region information as well"
        },
        "otherCitizenships": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string"
          }
        },
        "gender": {
          "enum": [ "male", "female", "not specified" ]
        },
        "label": {
          "type": "string",
          "description": "e.g. Web Developer"
        },
        "picture": {
          "type": "string",
          "description": "URL (as per RFC 3986) to a picture in JPEG or PNG format"
        },
        "email": {
          "type": "string",
          "description": "e.g. thomas@gmail.com",
          "format": "email"
        },
        "phone": {
          "type": "string",
          "description": "Phone numbers are stored as strings so use any format you like, e.g. 712-117-2923"
        },
        "url": {
          "type": "string",
          "description": "URL (as per RFC 3986) to your website, e.g. personal homepage",
          "format": "uri"
        },
        "summary": {
          "type": "string",
          "description": "Write a short 2-3 sentence biography about yourself"
        },
        "homeAddress": {
          "$ref": "http://json-schema.org/address"
        },
        "officeLocation": {
          "$ref": "http://json-schema.org/address"
        },
        "profiles": {
          "type": "array",
          "description": "Specify any number of social networks that you participate in",
          "additionalItems": false,
          "items": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
              "network": {
                "type": "string",
                "description": "e.g. Facebook or Twitter"
              },
              "username": {
                "type": "string",
                "description": "e.g. neutralthoughts"
              },
              "url": {
                "type": "string",
                "description": "e.g. http://twitter.com/neutralthoughts"
              }
            }
          }
        }
      }
    },
