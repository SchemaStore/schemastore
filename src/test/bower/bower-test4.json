{
	"$schema": "https://json.schemastore.org/bower",

	"name": "bootstrap",
	"version": "3.0.3",
	"main": [
		"./dist/js/bootstrap.js",
		"./dist/css/bootstrap.css",
		"./dist/fonts/glyphicons-halflings-regular.eot",
		"./dist/fonts/glyphicons-halflings-regular.svg",
		"./dist/fonts/glyphicons-halflings-regular.ttf",
		"./dist/fonts/glyphicons-halflings-regular.woff"
	],
	"ignore": [
		"**/.*",
		"_*",
		"docs-assets",
		"examples",
		"/fonts",
		"js/tests",
		"CNAME",
		"CONTRIBUTING.md",
		"Gruntfile.js",
		"browserstack.json",
		"composer.json",
		"package.json",
		"*.html"
	],
	"dependencies": {
		"jquery": ">= 1.9.0"
	},
	"moduleType": [
		"es6", "globals"
	]
}
