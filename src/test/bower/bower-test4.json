{
  "$schema": "https://json.schemastore.org/bower",
  "dependencies": {
    "jquery": ">= 1.9.0"
  },
  "ignore": [
    "**/.*",
    "_*",
    "docs-assets",
    "examples",
    "/fonts",
    "js/tests",
    "CNAME",
    "CONTRIBUTING.md",
    "Gruntfile.js",
    "browserstack.json",
    "composer.json",
    "package.json",
    "*.html"
