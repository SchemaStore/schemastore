{
  "$schema": "http://adctv-backend.herokuapp.com/componentSchema.json",
  "version": 2,
  "customElementsVersion": 1,
  "name": "My Component",
  "tagName": "my-component",
  "type": "my-component",
  "description": "Custom Web Element for GWD",
  "files": {
    "css": ["my-component.css"],
    "js": ["my-component.js"]
  },
  "attributes": [
    {
      "name": "my-attribute",
      "type": "string",
      "description": "My Attribute",
      "default": "My Attribute"
    }
  ],
  "events": [
    {
      "name": "my-event",
      "description": "My Event",
      "parameters": ["param1", "param2"]
    }
  ],
  "methods": [
    {
      "name": "my-method",
      "description": "My Method",
      "parameters": [
        {
          "name": "parameter",
          "label": "Parameter",
          "type": "string",
          "description": "My Parameter",
          "default": "hello"
        }
      ]
    }
  ],
  "nestable": true,
  "externalScripts": ["https://example.com/my-component.js"]
}
