{
  "$schema": "../../schemas/json/docker-extension-metadata.json",
  "icon": "extension-icon.svg",
  "ui": {
    "dashboard-tab": {
      "title": "MyTitle",
      "root": "/ui",
      "src": "index.html"
    }
  },
  "vm": {
    "image": "${DESKTOP_PLUGIN_IMAGE}",
    "composefile": "docker-compose.yml"
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/darwin/binary"
          }
        ],
        "windows": [
          {
            "path": "/windows/binary.exe"
          }
        ],
        "linux": [
          {
            "path": "/linux/binary"
          }
        ]
      }
    ]
  }
}
