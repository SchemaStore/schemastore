{
  "$schema": "https://json.schemastore.org/coffeelint",
  "arrow_spacing": {
    "level": "ignore",
    "name": "arrow_spacing"
  },
  "camel_case_classes": {
    "level": "error",
    "name": "camel_case_classes"
  },
  "coffeescript_error": {
    "level": "error"
  },
  "colon_assignment_spacing": {
    "level": "ignore",
    "name": "colon_assignment_spacing",
    "spacing": {
      "left": 0,
      "right": 0
    }
  },
  "cyclomatic_complexity": {
    "level": "ignore",
    "name": "cyclomatic_complexity",
    "value": 10
  },
  "duplicate_key": {
    "level": "error",
    "name": "duplicate_key"
  },
  "empty_constructor_needs_parens": {
    "level": "ignore",
    "name": "empty_constructor_needs_parens"
  },
  "indentation": {
    "level": "error",
    "name": "indentation",
    "value": 2
  },
  "line_endings": {
    "level": "ignore",
    "name": "line_endings",
    "value": "unix"
  },
  "max_line_length": {
    "level": "error",
    "limitComments": true,
    "name": "max_line_length",
    "value": 80
  },
  "missing_fat_arrows": {
    "level": "ignore",
    "name": "missing_fat_arrows"
  },
  "newlines_after_classes": {
    "level": "ignore",
    "name": "newlines_after_classes",
    "value": 3
  },
  "no_backticks": {
    "level": "error",
    "name": "no_backticks"
  },
  "no_empty_param_list": {
    "level": "ignore",
    "name": "no_empty_param_list"
  },
  "no_implicit_braces": {
    "level": "ignore",
    "name": "no_implicit_braces",
    "strict": true
  },
  "no_implicit_parens": {
    "level": "ignore",
    "name": "no_implicit_parens"
  },
  "no_plusplus": {
    "level": "ignore",
    "name": "no_plusplus"
  },
  "no_stand_alone_at": {
    "level": "ignore",
    "name": "no_stand_alone_at"
  },
  "no_tabs": {
    "level": "error",
    "name": "no_tabs"
  },
  "no_throwing_strings": {
    "level": "error",
    "name": "no_throwing_strings"
  },
  "no_trailing_semicolons": {
    "level": "error",
    "name": "no_trailing_semicolons"
  },
  "no_trailing_whitespace": {
    "allowed_in_comments": false,
    "allowed_in_empty_lines": true,
    "level": "error",
    "name": "no_trailing_whitespace"
  },
  "no_unnecessary_fat_arrows": {
    "level": "warn",
    "name": "no_unnecessary_fat_arrows"
  },
  "non_empty_constructor_needs_parens": {
    "level": "ignore",
    "name": "non_empty_constructor_needs_parens"
  },
  "space_operators": {
    "level": "ignore",
    "name": "space_operators"
  }
}
