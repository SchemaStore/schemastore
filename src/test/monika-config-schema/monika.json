{
  "probes": [
    {
      "alerts": [
        {
          "message": "response status message",
          "query": "response.status == 500"
        },
        {
          "message": "response time message",
          "query": "response.time > 150"
        }
      ],
      "description": "Multiple",
      "id": "1",
      "incidentThreshold": 3,
      "interval": 10,
      "name": "Probing Github",
      "recoveryThreshold": 3,
      "requests": [
        {
          "method": "GET",
          "saveBody": false,
          "timeout": 7000,
          "url": "https://github.com/"
        },
        {
          "method": "GET",
          "saveBody": true,
          "timeout": 7000,
          "url": "https://github.com/hyperjumptech"
        }
      ]
    }
  ]
}
