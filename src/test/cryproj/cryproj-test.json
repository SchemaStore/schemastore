{
    "$schema": "https://json.schemastore.org/cryproj",
    "version": 1,
    "type": "",
    "info": {
        "name": "Blank Game Template"
    },
    "content": {
        "assets": [
            "Assets"
        ],
        "code": [
            "Code"
        ]
    },
    "require": {
        "engine": "engine-5.4",
        "plugins": [
            {
                "type": "EPluginType::Native",
                "path": "CryDefaultEntities"
            },
            {
                "type": "EPluginType::Native",
                "path": "CrySensorSystem"
            },
            {
                "type": "EPluginType::Native",
                "path": "CryPerceptionSystem"
            },
            {
                "type": "EPluginType::Native",
                "path": "bin/win_x64/Game.dll"
            }
        ]
    },
    "console_variables": [
        {
            "name": "ca_UseIMG_CAF",
            "value": "0"
        },
        {
            "name": "sys_target_platforms",
            "value": "pc,ps4,xboxone,linux"
        }
    ]
}