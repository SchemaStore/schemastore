{
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw",
        "Notifications": {
          "Email": "your@email.here"
        }
      },
      "Global": {
        "DefaultUILanguage": "en-us",
        "HideTopLevelNodeFromPath": true,
        "TimeOutInMinutes": 20,
        "UmbracoPath": "~/umbraco",
        "UseHttps": false
      },
      "Hosting": {
        "Debug": false
      },
      "KeepAlive": {
        "DisableKeepAliveTask": false,
        "KeepAlivePingUrl": "{umbracoApplicationUrl}/api/keepalive/ping"
      },
      "ModelsBuilder": {
        "AcceptUnsafeModelsDirectory": false,
        "DebugLevel": 10,
        "FlagOutOfDateModels": true,
        "ModelsDirectory": "~/umbraco/models",
        "ModelsMode": "InMemoryAuto",
        "ModelsNamespace": "Umbraco.Cms.Web.Common.PublishedModels"
      },
      "RequestHandler": {
        "ConvertUrlsToAscii": "try"
      },
      "RuntimeMinification": {
        "dataFolder": "umbraco/Data/TEMP/Smidge",
        "version": "637510451273675926"
      },
      "Security": {
        "HideDisabledUsersInBackoffice": false,
        "KeepUserLoggedIn": false,
        "MemberPassword": {
          "MaxFailedAccessAttemptsBeforeLockout": 5,
          "RequireDigit": false,
          "RequireLowercase": false,
          "RequireNonLetterOrDigit": false,
          "RequireUppercase": false,
          "RequiredLength": 10
        },
        "UserPassword": {
          "MaxFailedAccessAttemptsBeforeLockout": 5,
          "RequireDigit": false,
          "RequireLowercase": false,
          "RequireNonLetterOrDigit": false,
          "RequireUppercase": false,
          "RequiredLength": 10
        },
        "UsernameIsEmail": true
      },
      "Tours": {
        "EnableTours": true
      }
    }
  }
}
