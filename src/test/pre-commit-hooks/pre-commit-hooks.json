[
  {
    "id": "foo",
    "name": "Foo",
    "entry": "foo-hook --opt",
    "language": "python",
    "files": "^foo_*\\.txt$",
    "exclude": "^foo_bar\\.txt$",
    "types": ["file"],
    "types_or": ["file"],
    "exclude_types": ["python"],
    "always_run": true,
    "fail_fast": true,
    "verbose": true,
    "pass_filenames": false,
    "require_serial": true,
    "description": "Foo hook.",
    "language_version": "3.11",
    "minimum_pre_commit_version": "2.19.0",
    "args": ["--bar", "baz"],
    "stages": ["post-commit", "post-merge"]
  },
  {
    "id": "bar",
    "name": "bar",
    "entry": "bar_hook",
    "language": "rust"
  }
]
