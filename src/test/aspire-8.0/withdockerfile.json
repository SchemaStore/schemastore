{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "goversion": {
      "inputs": {
        "value": {
          "type": "string"
        }
      },
      "type": "parameter.v0",
      "value": "{goversion.inputs.value}"
    },
    "mycontainer": {
      "build": {
        "args": {
          "GO_VERSION": "{goversion.value}"
        },
        "context": "qots",
        "dockerfile": "qots/Dockerfile",
        "secrets": {
          "SECRET_ASENV": {
            "type": "env",
            "value": "{secret.value}"
          },
          "SECRET_ASFILE": {
            "source": "Program.cs",
            "type": "file"
          }
        }
      },
      "type": "container.v1"
    },
    "secret": {
      "inputs": {
        "value": {
          "secret": true,
          "type": "string"
        }
      },
      "type": "parameter.v0",
      "value": "{secret.inputs.value}"
    }
  }
}
