{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "terminfo",
  "patterns": [
    {
      "include": "#comments"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "match": "^#.*$",
          "name": "comment"
        }
      ]
    }
  },
  "scopeName": "source.terminfo"
}
