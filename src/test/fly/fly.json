{
  "app": "foobar",
  "build": {
    "args": {
      "key": "value"
    }
  },
  "experimental": {
    "allowed_public_ports": [],
    "auto_rollback": true,
    "cmd": "foobar.sh",
    "entrypoint": "sh"
  },
  "kill_signal": "SIGINT",
  "kill_timeout": 5,
  "mounts": {
    "destination": "/data",
    "source": "data1"
  },
  "processes": [],
  "vm": [
    {
      "cpu_kind": "performance",
      "cpus": 4,
      "gpu_kind": "a100-pcie-40gb",
      "gpus": 1,
      "host_dedication_id": "customer-id",
      "kernel_args": "no-hlt=true",
      "memory": "2gb",
      "processes": ["app"],
      "size": "shared-cpu-2x"
    }
  ]
}
