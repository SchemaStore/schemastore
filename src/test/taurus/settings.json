{
  "settings": {
    "artifacts-dir": "/tmp/%H%M%S",
    "aggregator": "consolidator",
    "default-executor": "jmeter",
    "check-interval": 1,
    "proxy": {
      "address": "http://127.0.0.1:8080",
      "username": "user",
      "password": "12345",
      "ssl-cert": "path/to/cert",
      "ssl-client-cert": "path/to/cert"
    },
    "check-updates": true,
    "verbose": false,
    "env": {
      "VARNAME1": "VARVALUE1",
      "VARNAME2": "VARVALUE2"
    }
  }
}
