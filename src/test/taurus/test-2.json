{
  "aggregator": "aggregator",
  "execution": [
    {
      "executor": "jmeter",
      "scenario": "jmx_sample"
    }
  ],
  "scenarios": {
    "jmx_sample": {
      "script": "tests/jmx/dummy.jmx"
    }
  },
  "modules": {
    "jmeter": {
      "class": "bzt.modules.jmeter.JMeterExecutor",
      "path": "build/jmeter/bin/jmeter",
      "properties": {
        "jmeter.save.saveservice.autoflush": "true"
      }
    }
  },
  "provisioning": "local",
  "reporting": [
    {
      "module": "status"
    }
  ]
}
