{
  "dependentFileProviders": {
    "remove": "*",
    "add": {
      "addedExtension": {},
      "pathSegment": {
        "remove": "*",
        "add": {
          ".*": [
            ".js",
            ".css",
            ".html",
            ".htm",
            ".less",
            ".scss",
            ".coffee",
            ".iced",
            ".config",
            ".cs",
            ".vb",
            ".json"
          ]
        }
      },
      "extensionToExtension": {
        "remove": "*",
        "add": {
          ".js": [".coffee", ".iced", ".ts", ".tsx", ".jsx"],
          ".css": [".less", ".scss", ".sass", ".styl"],
          ".html": [".md", ".mdown", ".markdown", ".mdwn"],
          ".map": [".js", ".css"],
          ".svgz": [".svg"],
          ".designer.cs": [".resx"],
          ".cs.d.ts": [".cs"]
        }
      },
      "fileToFile": {
        "add": {
          ".bowerrc": ["bower.json"],
          ".npmrc": ["package.json"],
          "npm-shrinkwrap.json": ["package.json"],
          "yarn.lock": ["package.json"],
          ".yarnclean": ["package.json"],
          ".yarnignore": ["package.json"],
          ".yarn-integrity": ["package.json"],
          ".yarnrc": ["package.json"]
        }
      },
      "filePartToExtension": {
        "add": {
          "-vsdoc.js": [".js"]
        }
      },
      "allExtensions": {
        "remove": "*",
        "add": {
          ".*": [".tt"]
        }
      }
    }
  },
  "root": true
}
