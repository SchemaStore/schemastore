{
  "name": "pattern",
  "version": "2.0.0",
  "contributions": {
    "html": {
      "attributes": [
        {
          "pattern": {
            "delegate": true,
            "or": [
              [
                "(",
                "#item",
                ")"
              ],
              [
                {
                  "priority": "lowest",
                  "template": [
                    "on-"
                  ]
                },
                "#...",
                "#item"
              ]
            ]
          }
        }
      ]
    }
  }
}
