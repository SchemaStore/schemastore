{
    "manifest_version": 2,
    "name": "IRC Protocol Handler",
    "description": "Demos *incorrect* usage of protocol_handlers - see https://github.com/mdn/webextensions-examples/blob/master/open-irc-links/manifest.json",
    "version": "1.0.0",
    "icons": {
      "64": "icons/icon.png"
    },
    "protocol_handlers": {
        "protocol": "irc",
        "name": "IRC Mibbit Extension",
        "uriTemplate": "https://mibbit.com/?url=%s"
    }
  }